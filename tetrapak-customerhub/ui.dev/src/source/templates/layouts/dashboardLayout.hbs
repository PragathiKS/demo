<!DOCTYPE html>
<html lang="en">
<sly data-sly-use.{{fsdId}}Model="ux-model.js" data-sly-use.lib="/apps/tp-customerhub/components/{{fsdId}}/{{fsdId}}-template.html" />
<sly data-sly-test="${ {{fsdId}}Model.requestURI == {{fsdId}}Model.nodePathWithSelector} || {{fsdId}}Model.requestURI == {{fsdId}}Model.pagePathWithSelector}">
    <head>
        <sly data-sly-use.clientLib="/libs/granite/sightly/templates/clientlib.html" />
        <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="template" content="demoTemplate">
        <meta name="pageType" content="GenericPageType">
        <title>{{fsdId}} Component | UpFront</title>
        <sly data-sly-call="${clientLib.css @ categories='tp.customerhub.vendor'}" />
        <sly data-sly-call="${clientLib.css @ categories='tp.customerhub.default.en'}" />
    </head>
</sly>

<body>
    <div class="tp__container">
        <div class="row">
            {{!-- Header goes here --}}
            <div class="col-lg-2">
                {{!-- Side nav goes here --}}
            </div>
            <div class="col-lg-6">
                {{!-- Main content goes here --}}
                {{> body}}
            </div>
            <div class="col-lg-4">
                {{!-- Notification panel goes here --}}
            </div>
        </div>
    </div>

    <sly data-sly-test="${ {{fsdId}}Model.requestURI == {{fsdId}}Model.nodePathWithSelector} || {{fsdId}}Model.requestURI == {{fsdId}}Model.pagePathWithSelector}"
        data-sly-call="${clientLib.js @ categories='tp.customerhub.vendor'}" />
    <sly data-sly-test="${ {{fsdId}}Model.requestURI == {{fsdId}}Model.nodePathWithSelector} || {{fsdId}}Model.requestURI == {{fsdId}}Model.pagePathWithSelector}"
        data-sly-call="${clientLib.js @ categories='tp.customerhub.global'}" />
</body>
</html>
