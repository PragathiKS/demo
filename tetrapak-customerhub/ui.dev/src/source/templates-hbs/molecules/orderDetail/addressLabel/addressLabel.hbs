<div class="tpmol-order-delivery-summary__delivery-detail row">
    <div class="tpmol-order-delivery-summary__delivery-detail-label col-lg">{{i18n key=label}}</div>
    <div class="col-lg">
        {{#if link}}
            {{> atoms/links/link/link text=value href=linkUrl iconClass="icon-External_Link" isExternal=true jsClass=linkJsClass data-delivery-number=data-delivery-number }}
        {{else}}
            {{value}}
        {{/if}}
    </div>
</div>
