{{#if isError}}
    <div class="tp-ordering-card__no-data">
        {{#if labels.dataError}}
            {{> atoms/error/error error=true message=labels.dataError}}
        {{else}}
            {{> atoms/error/error error=true message="cuhu.error.message"}}
        {{/if}}
    </div>
{{else}}
    {{#if orderHeadings}}
        {{> molecules/table/table baseClass="tp-ordering-card" jsClass="js-ordering-card" list=orders headings=orderHeadings}}
    {{else}}
        <div class="tp-ordering-card__no-data">{{i18n key=labels.noData}}</div>
    {{/if}}
{{/if}}
