        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataValue">{{equipData.constructionYear}}</div>
    </div>
    <div class="tp-equipment-details__details-line">
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataKey">{{i18n key=i18nKeys.customerWarrantyEndDate}}</div>
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataValue">{{equipData.customerWarrantyEndDate}}</div>
