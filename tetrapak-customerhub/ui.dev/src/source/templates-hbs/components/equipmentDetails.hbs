<div class="tp-equipment-details__container">
    <a href="#" class="tp-equipment-details__btn tp-equipment-details__back-btn js-equipment-details__back-btn" auto_locator="backToEquipmentList">
          <i class="icon icon-Chevron_Left"></i>
          {{i18n key=i18nKeys.backToEquipmentList}}
    </a>
</div>
<h2 class="tp-equipment-details__title" auto_locator="equipmentHeading">
    {{#if isError}}
        No equipment data fetched
    {{else}}
        <span class="tp-equipment-details__title__bold">{{equipData.equipmentName}}</span>- {{equipData.serialNumber}}
    {{/if}}
</h2>
<div class="row tp-equipment-details__info-card-row">
    <div class="tp-equipment-details__info-card-col">
        <div class="tp-equipment-details__info-card js-equipment-details__info-card">
            <h3 auto_locator="sectionTitle">{{i18n key=i18nKeys.equipmentInformation}}</h3>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.country}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.countryName}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.location}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.location}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.site}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.site}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.line}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.lineCode}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="functionalLocationDescDataKey">{{i18n key=i18nKeys.functionalLocationDesc}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="functionalLocationDescValue">{{equipData.functionalLocationDesc}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.equipmentStatus}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.equipmentStatusDesc}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.position}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.position}}</div>
            </div>
            <div class="tp-equipment-details__info-line">
                <div class="tp-equipment-details__info-cell-1" auto_locator="equipmentDataKey">{{i18n key=i18nKeys.equipmentDescription}}</div>
                <div class="tp-equipment-details__info-cell-2" auto_locator="equipmentDataValue">{{equipData.equipmentName}}</div>
            </div>
            {{#if isError}}
                {{#if message}}
                    {{> atoms/error/error error=true message=message}}
                {{else}}
                    {{> atoms/error/error error=true message="cuhu.error.message"}}
                {{/if}}
            {{else}}
                <div class="tp-equipment-details__action-wrap">
                    {{> atoms/buttons/button/button iconClass="icon-Edit" modifier="transparent" text=i18nKeys.updateEquipmentInformation baseClass="tp-equipment-details__btn tp-equipment-details__update" jsClass="js-equipment-details__update"}}
                </div>
                <div class="tp-equipment-details__note">
                    <span>{{i18n key=i18nKeys.equipmentDetails-statusUpdateHeading}}:</span>
                    {{i18n key=i18nKeys.equipmentDetails-statusUpdateDescription}}
                </div>
            {{/if}}
        </div>
    </div>
    <div class="js-equipment-details__info-links tp-equipment-details__info-links">
        <div class="tp-equipment-details__info-links__item">
            <h4>Technical Publications</h4>
            <div class="tp-equipment-details__info-links__item__link">
                <a href="#">All publications</a>
                <i class="icon icon-Chevron_Right tp-equipment-details__info-links__item__icon"></i>
            </div>
        </div>
    </div>
