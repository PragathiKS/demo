    <div class="tp-equipment-details__details-line">
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataKey">{{i18n key=i18nKeys.customerWarrantyStartDate}}</div>
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataValue">{{equipData.customerWarrantyStartDate}}</div>
