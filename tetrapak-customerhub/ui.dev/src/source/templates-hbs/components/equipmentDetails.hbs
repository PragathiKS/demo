        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataValue">{{equipData.superiorEquipment}}</div>
    </div>
    <div class="tp-equipment-details__details-line">
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataKey">{{i18n key=i18nKeys.manufacturer}}</div>
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataValue">{{equipData.manufacturer}}</div>
    </div>
    <div class="tp-equipment-details__details-line">
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataKey">{{i18n key=i18nKeys.manufacturerCountry}}</div>
        <div class="tp-equipment-details__details-cell" auto_locator="moreEquipmentDataValue">{{equipData.manufacturerCountry}} | {{equipData.manufacturerCountryName}}</div>
