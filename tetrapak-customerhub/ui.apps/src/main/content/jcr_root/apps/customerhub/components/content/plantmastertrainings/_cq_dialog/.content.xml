<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Plant Master Trainings"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
					<column1
                            jcr:primaryType="nt:unstructured"
							jcr:title="Plant Master Trainings Email Configuration"
							maximized="{Boolean}true"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
							<content
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<title
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Component title (i18N key)"
										name="./title"
										required="true"/>
									<availabletrainings
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Available Trainings label (i18N key)"
										name="./availableTrainings"
										required="true"/>
									<learninghistory
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Learning History label (i18N key)"
										name="./learningHistory"
										required="true"/>
									<trainingmaterialhandouts
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Training Material Handouts label (i18N key)"
										name="./trainingMaterialHandouts"
										required="true"/>
									<coursedescription
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Course Description label (i18N key)"
										name="./courseDescription"
										required="true"/>
									<principleobjectives
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Principle Objectives label (i18N key)"
										name="./principleObjectives"
										required="true"/>
									<targetgroups
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Target Groups label (i18N key)"
										name="./targetGroups"
										required="true"/>
									<duration
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Duration label (i18N key)"
										name="./duration"
										required="true"/>                           
									<maxparticipants
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Max Participants label (i18N key)"
										name="./maxParticipants"
										required="true"/>
									<knowledgerequirements
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Knowledge Requirements (i18N key)"
										name="./knowledgeRequirements"
										required="true"/>
									<noofparticipants
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Number of Participants label (i18N key)"
										name="./noOfParticipantsLabel"
										required="true"/>
									<preferredlocation
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Preferred Location label (i18N key)"
										name="./preferredLocationLabel"
										required="true"/>
									<preferreddate
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Preferred Date label (i18N key)"
										name="./preferredDateLabel"
										required="true"/>
									<comments
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Comments label (i18N key)"
										name="./commentsLabel"/>
									<confirmationtext
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Confirmation Text"
										name="./confirmationText"
										required="true"/>
									<submitButtonLabel
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Submit Button label (i18N key)"
										name="./submitButtonLabel"
										required="true"/>                          
									<inputErrorMsg
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Input error message (i18N key)"
										name="./inputErrorMsg"
										required="true"/>
									<successMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Success page message (i18N key)"
										name="./successMessage"
										required="true"/>
									<formTitle
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Form Title label (i18N key)"
										name="./formTitle"
										required="true"/>
									<consentLabel
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Consent label (i18N key)"
										name="./consentLabel"
										required="true"/>	
									<trainingNameLabel
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Training Name label (i18N key)"
										name="./trainingNameLabel"
										required="true"/>	
									<trainingIdLabel
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Training Id label (i18N key)"
										name="./trainingIdLabel"/>									
								</items>
							</content>
						</items>
                    </column1>
					<column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Plant Master Trainings Email Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<content
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
									<items jcr:primaryType="nt:unstructured">
										<subject
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
											fieldLabel="Subject text"
											name="./subject"/>
										<salutation
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
											fieldLabel="Salutation text"
											name="./salutation"/>
										<body
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
											fieldLabel="Body text"
											name="./body"/>
										<contactDetails
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
											fieldLabel="Contact Details label (i18N key)"
											name="./contactDetails"/>
									</items>
							</content>
                        </items>
                    </column2>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
