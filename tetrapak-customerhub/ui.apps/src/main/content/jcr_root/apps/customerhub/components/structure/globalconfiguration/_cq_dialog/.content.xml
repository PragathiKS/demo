<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Global Configurations"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <section
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Left Navigation"
                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <navHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Left Navigation Heading i18 Key"
                                    fieldLabel="Left Navigation Heading i18 Key"
                                    name="./navHeadingI18n"/>
                            <closeBtnText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Close Button Text i18 Key"
                                    fieldLabel="Close Button Text i18 Key"
                                    name="./closeBtnText"/>
                        </items>
                    </section>
                    <stickySection
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Tetrapak Home Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <href
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Select Public Web Home Page Path"
                                    fieldLabel="Public Web Home Page"
                                    rootPath="/content/tetrapak/customerhub"
                                    name="./stickyHref"/>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Tetrapak home i18 Key"
                                    fieldLabel="Tetrapak home i18 Key"
                                    name="./stickyLabel"/>
                            <iconClass
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    emptyText="Select"
                                    fieldLabel="Icon Class"
                                    name="./stickyIconClass"
                                    translateOptions="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <None jcr:primaryType="nt:unstructured"
                                               text="None"
                                               value=""/>
                                    <Dashboard jcr:primaryType="nt:unstructured"
                                               text="Dashboard"
                                               value="icon-Dashboard"/>
                                    <Install jcr:primaryType="nt:unstructured"
                                             text="Install"
                                             value="icon-Install"/>
                                    <Packaging_Design jcr:primaryType="nt:unstructured"
                                                      text="Packaging_Design"
                                                      value="icon-Packaging_Design"/>
                                    <Cart jcr:primaryType="nt:unstructured"
                                          text="Cart"
                                          value="icon-Cart"/>
                                    <Inventory jcr:primaryType="nt:unstructured"
                                               text="Inventory"
                                               value="icon-Inventory"/>
                                    <Financials jcr:primaryType="nt:unstructured"
                                                text="Financials"
                                                value="icon-Financials"/>
                                    <Training jcr:primaryType="nt:unstructured"
                                              text="Training"
                                              value="icon-Training"/>
                                    <Collaboration jcr:primaryType="nt:unstructured"
                                                   text="Collaboration"
                                                   value="icon-Collaboration"/>
                                    <Phone jcr:primaryType="nt:unstructured"
                                           text="Phone"
                                           value="icon-Phone"/>
                                    <Info jcr:primaryType="nt:unstructured"
                                          text="Info"
                                          value="icon-Info"/>
                                    <Logout jcr:primaryType="nt:unstructured"
                                            text="Logout"
                                            value="icon-Logout"/>
                                    <TetraPak jcr:primaryType="nt:unstructured"
                                          text="TetraPak"
                                          value="icon-TetraPak"/>
                                </items>
                            </iconClass>
                        </items>
                    </stickySection>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
