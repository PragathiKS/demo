<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Techical Publications"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="I18N Configurations"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                        	<technicalPublications
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="All Files Label (i18N key)"
                                name="./technicalPublications"
                                required="true"/>
                        	<allFiles
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="All Files Label (i18N key)"
                                name="./allFiles"
                                required="true"/>
                            <country
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country Label (i18N key)"
                                name="./country"
                                required="true"/>
                            <customer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Customer Label (i18N key)"
                                name="./customer"
                                required="true"/>        
                            <documentNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Document Number Label (i18N key)"
                                name="./documentNumber"
                                required="true"/>
                            <documentType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Document Type Label (i18N key)"
                                name="./documentType"
                                required="true"/>
                            <issueDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Issue Date Label (i18N key)"
                                name="./issueDate"
                                required="true"/>
                            <line
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Line Label (i18N key)"
                                name="./line"
                                required="true"/>
                            <lineEquipment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Line/Equipment Label (i18N key)"
                                name="./lineEquipment"
                                required="true"/> 
                            <materialNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Material Number Label (i18N key)"
                                name="./materialNumber"
                                required="true"/>    
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name Label (i18N key)"
                                name="./name"
                                required="true"/>               
                            <rebuildingKitNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Rebuilding Kit Number Label (i18N key)"
                                name="./rebuildingKitNumber"
                                required="true"/>
                            <rebuildingKitName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Rebuilding Kit Name Label (i18N key)"
                                name="./rebuildingKitName"
                                required="true"/>
                             <searchResults
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Search Results Label (i18N key)"
                                name="./searchResults"
                                required="true"/>    
                             <serialNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Serial Number Label (i18N key)"
                                name="./serialNumber"
                                required="true"/>
                              <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Description (i18N key)"
                                name="./description"
                                required="true"/> 
                        </items>
                    </column>
                </items>
            </tab1>
        </items>
    </content>
</jcr:root>
