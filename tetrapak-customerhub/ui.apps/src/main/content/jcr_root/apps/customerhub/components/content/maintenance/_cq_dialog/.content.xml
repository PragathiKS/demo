<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Maintenance Component"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <section
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Maintenance i18n keys"
                    sling:resourceType="granite/ui/components/foundation/section">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <siteFilterLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Site Filter Label i18n key"
                                    fieldLabel="Site Filter Label i18n key"
                                    name="./siteFilterLabel"
                                    required="true"/>
                            <lineFilterLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Line Filter Label i18n key"
                                    fieldLabel="Line Filter Label i18n key"
                                    name="./lineFilterLabel"
                                    required="true"/>
                            <equipmentFilterLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Equipment Filter Label i18n key"
                                    fieldLabel="Equipment Filter Label i18n key"
                                    name="./equipmentFilterLabel"
                                    required="true"/>
                            <allOptionText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter All Options i18n key"
                                    fieldLabel="All Options i18n key"
                                    name="./allOptionText"
                                    required="true"/>
                            <contactsHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Contacts Heading i18n key"
                                    fieldLabel="Contacts Heading i18n key"
                                    name="./contactsHeading"
                                    required="true"/>
                            <scheduleHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Schedule Heading i18n key"
                                    fieldLabel="Schedule Heading i18n key"
                                    name="./scheduleHeading"
                                    required="true"/>
                            <eventsHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Events Heading i18n key"
                                    fieldLabel="Events Heading i18n key"
                                    name="./eventsHeading"
                                    required="true"/>
                            <statusLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Status Label i18n key"
                                    fieldLabel="Status Label i18n key"
                                    name="./statusLabel"
                                    required="true"/>
                            <serviceAgreementLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Service Agreement Label i18n key"
                                    fieldLabel="Service Agreement Label i18n key"
                                    name="./serviceAgreementLabel"
                                    required="true"/>
                            <plannedDurationLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Planned Duration Label i18n key"
                                    fieldLabel="Planned Duration Label i18n key"
                                    name="./plannedDurationLabel"
                                    required="true"/>
                            <plannedStartLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Planned Start Label i18n key"
                                    fieldLabel="Planned Start Label i18n key"
                                    name="./plannedStartLabel"
                                    required="true"/>
                            <plannedFinishedLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Planned Finished Label i18n key"
                                    fieldLabel="Planned Finished Label i18n key"
                                    name="./plannedFinishedLabel"
                                    required="true"/>
                            <lineLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Line Label i18n key"
                                    fieldLabel="Line Label i18n key"
                                    name="./lineLabel"
                                    required="true"/>
                            <equipmentLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Equipment Label i18n key"
                                    fieldLabel="Equipment Label i18n key"
                                    name="./equipmentLabel"
                                    required="true"/>
                            <serviceOrderLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Service Order Label i18n key"
                                    fieldLabel="Service Order Label i18n key"
                                    name="./serviceOrderLabel"
                                    required="true"/>
                            <operationShortTextLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Operation Short Text Label i18n key"
                                    fieldLabel="Operation Short Text Label i18n key"
                                    name="./operationShortTextLabel"
                                    required="true"/>
                            <noDataMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter No Data Message i18n key"
                                    fieldLabel="No Data Message i18n key"
                                    name="./noDataMsg"
                                    required="true"/>
                            <errorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Error Message i18n key"
                                    fieldLabel="Error Message i18n key"
                                    name="./errorMsg"
                                    required="true"/>
                        </items>
                    </column>
                </items>
            </section>
        </items>
    </content>
</jcr:root>
