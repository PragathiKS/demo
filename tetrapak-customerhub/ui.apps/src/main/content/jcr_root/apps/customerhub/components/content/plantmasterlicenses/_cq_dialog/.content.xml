<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Request Engineering Licenses"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Request Engineering Licenses i18n Keys Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <userDetailsSectionTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="User details section title (i18N key)"
                                name="./userDetailsSectionTitle"
                                required="true"/>
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name field label (i18N key)"
                                name="./name"
                                required="true"/>
                            <namePlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name field placeholder text (i18N key)"
                                name="./namePlaceholder"
                                required="true"/>
                            <activationDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Activation date field label (i18N key)"
                                name="./activationDate"
                                required="true"/>
                            <activationDatePlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Activation date field placeholder (i18N key)"
                                name="./activationDatePlaceholder"
                                required="true"/>
                            <licenseSelectionSectionTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="License selection section title (i18N key)"
                                name="./licenseSelectionSectionTitle"
                                required="true"/>
                            <addUser
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Add user button label (i18N key)"
                                name="./addUser"
                                required="true"/>
                            <comments
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Comments field label (i18N key)"
                                name="./comments"
                                required="true"/>
                            <submitButton
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Submit button label (i18N key)"
                                name="./submitButton"
                                required="true"/>
                            <licenseDescriptions
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="License descriptions label (i18N key)"
                                name="./licenseDescriptions"
                                required="true"/>
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Description text (i18N key)"
                                name="./description"
                                required="true"/>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Request Engineering Licenses Email Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <subject
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subject (i18N key)"
                                name="./subject"/>
                            <salutation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Salutation text (i18N key)"
                                name="./salutation"/>
                            <body
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Body text (i18N key)"
                                name="./body"/>
                            <nameping
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name field label (i18N key)"
                                name="./nameping"
                                required="true"/>
                            <emailping
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Email field label (i18N key)"
                                name="./nameping"
                                required="true"/>
                        </items>
                    </column2>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
