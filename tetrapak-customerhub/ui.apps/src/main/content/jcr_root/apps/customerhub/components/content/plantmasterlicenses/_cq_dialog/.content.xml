                                                required="true"/>
                                            <removeUser
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Remove user button label (i18N key)"
                                                name="./engineeringLicense/removeUser"
                                                required="true"/>
                                            <comments
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Comments field label (i18N key)"
                                                name="./engineeringLicense/comments"
                                                required="true"/>
                                            <submitButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Submit button label (i18N key)"
                                                name="./engineeringLicense/submitButton"
                                                required="true"/>
                                            <licenseDescriptions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="License descriptions label (i18N key)"
                                                name="./engineeringLicense/licenseDescriptions"
                                                required="true"/>
                                            <successMessageHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Success message heading text (i18N key)"
                                                name="./engineeringLicense/successMessageHeading"
                                                required="true"/>
                                            <successMessageDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Success message description text (i18N key)"
                                                name="./engineeringLicense/successMessageDescription"
                                                required="true"/>
                                            <inputFieldError
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Input field error text (i18N key)"
                                                name="./engineeringLicense/inputFieldError"
