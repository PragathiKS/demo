                                                fieldLabel="HMI Type field label (i18N key)"
                                                name="./siteLicense/hmiType"
                                                required="true"/>
                                            <mesType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="MES Type field label (i18N key)"
                                                name="./siteLicense/mesType"
                                                required="true"/>
                                            <numberOfBasicUnit
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Number of Basic Unit field label (i18N key)"
                                                name="./siteLicense/numberOfBasicUnit"
                                                required="true"/>
                                            <basicUnitToolTipHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Number of basic unit tooltip heading (i18N key)"
                                                name="./siteLicense/basicUnitToolTipHeading"
                                                required="true"/>
                                            <basicUnitToolTipDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Number of basic unit tooltip description (i18N key)"
                                                name="./siteLicense/basicUnitToolTipDescription"
                                                required="true"/>
                                            <numberOfAdvancedUnit
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Number of Advanced Unit field label (i18N key)"
                                                name="./siteLicense/numberOfAdvancedUnit"
                                                required="true"/>
                                            <advancedUnitToolTipHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Number of advanced unit tooltip heading (i18N key)"
                                                name="./siteLicense/advancedUnitToolTipHeading"
                                                required="true"/>
                                            <advancedUnitToolTipDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
