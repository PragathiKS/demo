<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Documents Component"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<column jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/container"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<sitefilterlabel jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Site Filter Label i18n key"
						fieldLabel="Site Filter Label i18n key" name="./siteFilterLabel"
						required="true" />
					<linefilterlabel jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Line Filter Label i18n key"
						fieldLabel="Line Filter Label i18n key" name="./lineFilterLabel"
						required="true" />
					<documentheading jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Document Heading i18n key"
						fieldLabel="Document Heading i18n key" name="./documentHeading"
						required="true" />
					<documentlabel jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Document Label i18n key"
						fieldLabel="Document Label i18n key" name="./documentLabel"
						required="true" />
					<documenttypelabel
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Document Type Label i18n key"
						fieldLabel="Document Type Label i18n key"
						name="./documentTypeLabel" required="true" />
					<totaldocumentslabel
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Total Documents Label i18n key"
						fieldLabel="Total Documents Label i18n key"
						name="./totalDocumentsLabel" required="true" />
					<nodatamsg jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter no data msg i18n key"
						fieldLabel="No data msg i18n key" name="./noDataMsg"
						required="true" />
					<errormsg jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Enter Error msg i18n key"
						fieldLabel="Error msg i18n key" name="./errorMsg" required="true" />
					<techpubhost jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
						fieldDescription="Enter Techpub Host Link"
						fieldLabel="Techpub Host Link" required="true"
						rootPath="/content/tetrapak/customerhub" name="./techPubHost" />
				</items>
			</column>
		</items>
	</content>
</jcr:root>
