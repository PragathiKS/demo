<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Order Details Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <section1
                jcr:primaryType="nt:unstructured"
                jcr:title="Order Details Page Label Content"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <backToOrderHistory
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter back link label which takes back to the order history page.&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Back Link Label"
                                name="./backToOrderHistory"
                                required="true"/>
                            <orderNo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Order number Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Order Number Label"
                                name="./orderNo"
                                required="true"/>
                            <orderNoLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter URL for the Order number Link&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Order Number Link"
                                name="./orderNoLink"
                                required="false"/>
                            <customerNameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Customer Name Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Customer Name Label"
                                name="./customerNameLabel"
                                required="true"/>
                            <purchaseOrderNumberLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Purchase Order Number Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Purchase Order Number Label"
                                name="./purchaseOrderNumberLabel"
                                required="true"/>
                            <orderDateLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Order Date Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Order Date Label"
                                name="./orderDateLabel"
                                required="true"/>
                            <customerNumberLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Customer number Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Customer Number Label"
                                name="./customerNumberLabel"
                                required="true"/>
                            <customerReferenceLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Customer Reference Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Customer Reference Label"
                                name="./customerReferenceLabel"
                                required="true"/>
                            <webReferenceLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter link on click of Web reference Label.&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Web Reference Link"
                                name="./webReferenceLink"
                                required="false"/>
                            <webRefLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Web Reference Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Web Reference Label"
                                name="./webRefLabel"
                                required="true"/>
                            <custSupCentreLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Customer Support Centre Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Customer Support Centre Label"
                                name="./custSupCentreLabel"
                                required="true"/>
                            <createExcelLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Create Excel Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Create Excel Label"
                                name="./createExcelLabel"
                                required="true"/>
                            <createPdfLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Create Pdf Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Create Pdf Label"
                                name="./createPdfLabel"
                                required="true"/>
                            <deliveryNumberLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Delivery Number Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Delivery Number Label"
                                name="./deliveryNumberLabel"
                                required="true"/>
                            <shippingLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Shipping Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Shipping Label"
                                name="./shippingLabel"
                                required="true"/>
                            <trackOrderLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Track Order Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Track Order Label"
                                name="./trackOrderLabel"
                                required="true"/>
                            <deliveryAddrLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Delivery Address Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Delivery Address Label"
                                name="./deliveryAddrLabel"
                                required="true"/>
                            <invoiceAddrLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Invoice Address Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Invoice Address Label"
                                name="./invoiceAddrLabel"
                                required="true"/>
                            <totalWeightLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Total Weight Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Total Weight Label"
                                name="./totalWeightLabel"
                                required="true"/>
                            <totalPricePreVatLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Total Price Pre-VAT Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Total Price Pre-VAT Label"
                                name="./totalPricePreVatLabel"
                                required="true"/>
                            <totalVatLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Total VAT Label&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Total VAT Label"
                                name="./totalVatLabel"
                                required="true"/>
                            <deliveryOrder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Delivery Order Label&#xa;Used on Package order details page."
                                fieldLabel="Delivery Order Label"
                                name="./deliveryOrder"
                                required="true"/>
                            <deliveryNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Delivery Number&#xa;Used on Package order details page."
                                fieldLabel="Delivery Number"
                                name="./deliveryNumber"
                                required="true"/>
                            <productionPlace
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Production Place Label&#xa;Used on Package order details page."
                                fieldLabel="Production Place Label"
                                name="./productionPlace"
                                required="true"/>
                            <requested
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Label for requested&#xa;Used on Package order details page."
                                fieldLabel="Requested Label"
                                name="./requested"
                                required="true"/>
                            <etd
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter ETD Label&#xa;Used on Package order details page."
                                fieldLabel="ETD Label"
                                name="./etd"
                                required="true"/>
                            <kpkPopUpHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Kilo Pack Quantity PopUp Heading Label&#xa;Used on Package order details page."
                                fieldLabel="Kilo Pack Quantity PopUp Heading"
                                name="./kpkPopUpHeading"
                                required="true"/>
                            <kpkPopUpDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Requested KPK Label&#xa;Used on Package order details page."
                                fieldLabel="Requested KPK Label"
                                name="./kpkPopUpDescription"
                                required="true"/>
							<kpkPopUpCloseBtnText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Kilo Pack PopUp Close Button Text Used on Package order details page."
                                fieldLabel="Kilo Pack PopUp Close Button Text"
                                name="./kpkPopUpCloseBtnText"
                                required="true"/>
                            <deliveryProductsFilterCheckboxText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Delivery Products Filter Checkbox Text Used on Package order details page."
                                fieldLabel="Delivery Products Filter Checkbox Text"
                                name="./deliveryProductsFilterCheckboxText"
                                required="true"/>
                            <errorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Error Message in case an error occurs on order details page,&#xa;Used on both Parts and Package order details page."
                                fieldLabel="Error message"
                                name="./errorMsg"
                                required="false"/>
                            <noDataMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter error message incase the data is not available from API, Used on both Parts and Package order details page."
                                fieldLabel="No Data Error Message"
                                name="./noDataMsg"
                                required="false"/>
							<orderStatus
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter order status label used in the excel generation.&#xa;Used on both Parts and Package order details page excel."
                                fieldLabel="Order Status Label"
                                name="./orderStatus"
                                required="true"/>
                        </items>
                    </column>
                </items>
            </section1>
            <section2
                jcr:primaryType="nt:unstructured"
                jcr:title="Table Column Configurations"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <fieldset1
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Parts Delivery Table"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <partsDeliveryTableCols
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Parts Delivery table column names' ordered in a comma separated list. eg. sno,orderno,..."
                                        fieldLabel="Column List"
                                        name="./partsDeliveryTableCols"
                                        required="true"/>
                                </items>
                            </fieldset1>
                            <fieldset2
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Packaging Delivery Table"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <packagingDeliveryTableCols
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Packaging Delivery table column names' ordered in a comma separated list eg. sno,orderno,..."
                                        fieldLabel="Column List"
                                        name="./packagingDeliveryTableCols"
                                        required="true"/>
                                </items>
                            </fieldset2>
                            <fieldset3
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Packaging Products Table"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <packagingProductsTableCols
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the packaging products table column names' ordered in a comma separated list. eg. sno,orderno,..."
                                        fieldLabel="Column List"
                                        name="./packagingProductsTableCols"
                                        required="true"/>
                                </items>
                            </fieldset3>
                        </items>
                    </column>
                </items>
            </section2>
        </items>
    </content>
</jcr:root>
