<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Order Details Dialog"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Order Details Page Label Content"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <backToOrderHistory
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter back link label which takes back to the order history page. Used on both Parts and Package order details page."
                                    fieldLabel="Back Link Label"
                                    name="./backToOrderHistory"
                                    required="true"/>
                            <orderNo
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Order number Label Used on both Parts and Package order details page."
                                    fieldLabel="Order Number Label"
                                    name="./orderNo"
                                    required="true"/>
                            <orderNoLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter URL for the Order number Link Used on both Parts and Package order details page."
                                    fieldLabel="Order Number Link"
                                    name="./orderNoLink"
                                    required="false"/>
                            <customerNameLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Customer Name Label Used on both Parts and Package order details page."
                                    fieldLabel="Customer Name Label"
                                    name="./customerNameLabel"
                                    required="true"/>
                            <purchaseOrderNumberLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Purchase Order Number Label Used on both Parts and Package order details page."
                                    fieldLabel="Purchase Order Number Label"
                                    name="./purchaseOrderNumberLabel"
                                    required="true"/>
                            <orderDateLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Order Date Label Used on both Parts and Package order details page."
                                    fieldLabel="Order Date Label"
                                    name="./orderDateLabel"
                                    required="true"/>
                            <customerNumberLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Customer number Label Used on both Parts and Package order details page."
                                    fieldLabel="Customer Number Label"
                                    name="./customerNumberLabel"
                                    required="true"/>
                            <customerReferenceLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Customer Reference Label Used on both Parts and Package order details page."
                                    fieldLabel="Customer Reference Label"
                                    name="./customerReferenceLabel"
                                    required="true"/>
                            <webRefLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Web Reference Label Used on both Parts and Package order details page."
                                    fieldLabel="Web Reference Label"
                                    name="./webRefLabel"
                                    required="true"/>
                            <custSupCentreLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Customer Support Centre Label Used on both Parts and Package order details page."
                                    fieldLabel="Customer Support Centre Label"
                                    name="./custSupCentreLabel"
                                    required="true"/>
                            <createExcelLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Create Excel Label Used on both Parts and Package order details page."
                                    fieldLabel="Create Excel Label"
                                    name="./createExcelLabel"
                                    required="true"/>
                            <createPdfLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Create Pdf Label Used on both Parts and Package order details page."
                                    fieldLabel="Create Pdf Label"
                                    name="./createPdfLabel"
                                    required="true"/>
                            <deliveryNumberLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Delivery Number Label Used on both Parts and Package order details page."
                                    fieldLabel="Delivery Number Label"
                                    name="./deliveryNumberLabel"
                                    required="true"/>
                            <shippingLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Shipping Label Used on both Parts and Package order details page."
                                    fieldLabel="Shipping Label"
                                    name="./shippingLabel"
                                    required="true"/>
                            <trackOrderLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Track Order Label Used on both Parts and Package order details page."
                                    fieldLabel="Track Order Label"
                                    name="./trackOrderLabel"
                                    required="true"/>
                            <deliveryAddrLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Delivery Address Label Used on both Parts and Package order details page."
                                    fieldLabel="Delivery Address Label"
                                    name="./deliveryAddrLabel"
                                    required="true"/>
                            <invoiceAddrLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Invoice Address Label Used on both Parts and Package order details page."
                                    fieldLabel="Invoice Address Label"
                                    name="./invoiceAddrLabel"
                                    required="true"/>
                            <totalWeightLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Total Weight Label Used on both Parts and Package order details page."
                                    fieldLabel="Total Weight Label"
                                    name="./totalWeightLabel"
                                    required="true"/>
                            <totalPricePreVatLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Total Price Pre-VAT Label Used on both Parts and Package order details page."
                                    fieldLabel="Total Price Pre-VAT Label"
                                    name="./totalPricePreVatLabel"
                                    required="true"/>
                            <totalVatLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Total VAT Label Used on both Parts and Package order details page."
                                    fieldLabel="Total VAT Label"
                                    name="./totalVatLabel"
                                    required="true"/>
                            <deliveryOrder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Delivery Order Label Used on Package order details page."
                                    fieldLabel="Delivery Order Label"
                                    name="./deliveryOrder"
                                    required="true"/>
                            <deliveryNumber
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Delivery Number Used on Package order details page."
                                    fieldLabel="Delivery Number"
                                    name="./deliveryNumber"
                                    required="true"/>
                            <productionPlace
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Production Place Label Used on Package order details page."
                                    fieldLabel="Production Place Label"
                                    name="./productionPlace"
                                    required="true"/>
                            <requested
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Label for requested Used on Package order details page."
                                    fieldLabel="Requested Label"
                                    name="./requested"
                                    required="true"/>
                            <etd
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter ETD Label Used on Package order details page."
                                    fieldLabel="ETD Label"
                                    name="./etd"
                                    required="true"/>
                            <kpkPopUpHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Kilo Pack Quantity PopUp Heading Label Used on Package order details page."
                                    fieldLabel="Kilo Pack Quantity PopUp Heading"
                                    name="./kpkPopUpHeading"
                                    required="true"/>
                            <kpkPopUpDescription
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Requested KPK Label Used on Package order details page."
                                    fieldLabel="Requested KPK Label"
                                    name="./kpkPopUpDescription"
                                    required="true"/>
                            <kpkPopUpCloseBtnText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Kilo Pack PopUp Close Button Text Used on Package order details page."
                                    fieldLabel="Kilo Pack PopUp Close Button Text"
                                    name="./kpkPopUpCloseBtnText"
                                    required="true"/>
                            <deliveryProductsFilterCheckboxText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Delivery Products Filter Checkbox Text Used on Package order details page."
                                    fieldLabel="Delivery Products Filter Checkbox Text"
                                    name="./deliveryProductsFilterCheckboxText"
                                    required="true"/>
                            <errorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter Error Message in case an error occurs on order details page, Used on both Parts and Package order details page."
                                    fieldLabel="Error message"
                                    name="./errorMsg"
                                    required="false"/>
                            <noDataMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter error message incase the data is not available from API, Used on both Parts and Package order details page."
                                    fieldLabel="No Data Error Message"
                                    name="./noDataMsg"
                                    required="false"/>
                            <orderStatus
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter order status label used in the excel generation. Used on both Parts and Package order details page excel."
                                    fieldLabel="Order Status Label"
                                    name="./orderStatus"
                                    required="true"/>
                            <fileDownloadErrorText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter File download error message i18n key"
                                    fieldLabel="File download error message i18n key"
                                    name="./fileDownloadErrorText"
                                    required="true"/>
                            <fileDownloadErrorClose
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter File download error close button i18n key"
                                    fieldLabel="File download error close button i18n key"
                                    name="./fileDownloadErrorClose"
                                    required="true"/>
                        </items>
                    </column1>
                    <column2
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Table Column Configurations"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <fieldset1
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Parts Delivery Table"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <partsDeliveryTableCols
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter the Parts Delivery table column names' ordered in a comma separated list. eg. sno,orderno,..."
                                            fieldLabel="Column List"
                                            name="./partsDeliveryTableCols"
                                            required="true"/>
                                </items>
                            </fieldset1>
                            <fieldset2
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Packaging Delivery Table"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <packagingDeliveryTableCols
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter the Packaging Delivery table column names' ordered in a comma separated list eg. sno,orderno,..."
                                            fieldLabel="Column List"
                                            name="./packagingDeliveryTableCols"
                                            required="true"/>
                                </items>
                            </fieldset2>
                            <fieldset3
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Packaging Products Table"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <packagingProductsTableCols
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter the packaging products table column names' ordered in a comma separated list. eg. sno,orderno,..."
                                            fieldLabel="Column List"
                                            name="./packagingProductsTableCols"
                                            required="true"/>
                                </items>
                            </fieldset3>
                        </items>
                    </column2>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
