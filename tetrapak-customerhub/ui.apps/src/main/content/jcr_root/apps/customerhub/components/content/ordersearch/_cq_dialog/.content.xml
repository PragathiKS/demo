<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Order Search History"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="order-search-history"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <resetButtonText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Reset Button Text i18 Key"
                            fieldLabel="Reset Button Text Key"
                            required="true"
                            name="./resetButtonTextI18n"/>
                    <searchButtonText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Search Button Text i18 Key"
                            fieldLabel="Search Button Text Key"
                            required="true"
                            name="./searchButtonTextI18n"/>
                    <allStatusesText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter All Status Text i18 Key"
                            fieldLabel="All Status Button Text Key"
                            required="true"
                            name="./allStatusesTextI18n"/>
                    <allAddressesText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter All Address Button Text i18 Key"
                            fieldLabel="All Address Text Key"
                            required="true"
                            name="./allAddressesTextI18n"/>
					<dateRangeLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Date Range Label i18 Key"
                            fieldLabel="Date Range Label Key"
                            required="true"
                            name="./dateRangeLabelI18n"/>
                    <deliveryAddressLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Delivery Address Label i18 Key"
                            fieldLabel="Delivery Address Label Key"
                            required="true"
                            name="./deliveryAddressLabelI18n"/>
                    <orderStatusLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Order Status Label i18 Key"
                            fieldLabel="Order Status Label Key"
                            required="true"
                            name="./orderStatusLabelI18n"/>
                    <searchInputLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Search Input Label i18 Key"
                            fieldLabel="Search Input Label Key"
                            required="true"
                            name="./searchInputLabelI18n"/>
                    <searchTermPlaceholder
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Search Term Placeholder i18 Key"
                            fieldLabel="Search Term Placeholder Key"
                            required="true"
                            name="./searchTermPlaceholderI18n"/>							
                </items>
            </column>
        </items>
    </content>
</jcr:root>
