<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Equipment"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="My equipment i18n Keys Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <country
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country Label (i18N key)"
                                name="./country"
                                required="true"/>
                            <line
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Line Label (i18N key)"
                                name="./line"
                                required="true"/>
                            <searchResults
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Search Results Label (i18N key)"
                                name="./searchResults"
                                required="true"/>
                            <site
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Label (i18N key)"
                                name="./site"
                                required="true"/>
                            <equipmentDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Description Label (i18N key)"
                                name="./equipmentDescription"
                                required="true"/>
                            <serialNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Serial Number Label (i18N key)"
                                name="./serialNumber"
                                required="true"/>
                            <equipmentStatus
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Status Label (i18N key)"
                                name="./equipmentStatus"
                                required="true"/>
                            <myEquipment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="My Equipment Label (i18N key)"
                                name="./myEquipment"
                                required="true"/>
                            <applyFilter
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Apply Filter Label (i18N key)"
                                name="./applyFilter"
                                required="true"/>
                            <removeAll
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Remove all Label (i18N key)"
                                name="./removeAll"
                                required="true"/>
                            <first
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="First Label (i18N key)"
                                name="./first"
                                required="true"/>
                            <last
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Last Label (i18N key)"
                                name="./last"
                                required="true"/>
							<hideAndShowCta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Hide And Show Cta Label (i18N key)"
                                name="./hideAndShowCta"
                                required="true"/>
                            <customizeTable
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Customize Table Label (i18N key)"
                                name="./customizeTable"
                                required="true"/>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tool tips Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <countryToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country Tool Tip Text (i18N key)"
                                name="./countryToolTip"/>
							<siteToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Tool Tip Text (i18N key)"
                                name="./siteToolTip"/>
							<lineToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Line Tool Tip Text (i18N key)"
                                name="./lineToolTip"/>
							<equipDescToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Description Tool Tip Text (i18N key)"
                                name="./equipDescToolTip"/>
							<serialNumToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Serial Number Tool Tip Text (i18N key)"
                                name="./serialNumToolTip"/>
							<equipStatToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Status Tool Tip Text (i18N key)"
                                name="./equipStatToolTip"/>						
                        </items>
                    </column2>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
