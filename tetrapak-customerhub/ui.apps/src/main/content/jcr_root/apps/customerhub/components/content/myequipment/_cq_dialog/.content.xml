<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Equipment"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="My equipment i18n Keys Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <country
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country Label (i18N key)"
                                name="./country"
                                required="true"/>
                            <line
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Line Label (i18N key)"
                                name="./line"
                                required="true"/>
                            <searchResults
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Search Results Label (i18N key)"
                                name="./searchResults"
                                required="true"/>
                            <site
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Label (i18N key)"
                                name="./site"
                                required="true"/>
                            <customer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Customer Label (i18N key)"
                                name="./customer"
                                required="true"/>
                            <equipmentDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Description Label (i18N key)"
                                name="./equipmentDescription"
                                required="true"/>
                            <serialNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Serial Number Label (i18N key)"
                                name="./serialNumber"
                                required="true"/>
                            <equipmentStatus
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Status Label (i18N key)"
                                name="./equipmentStatus"
                                required="true"/>
                            <equipmentType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Type Label (i18N key)"
                                name="./equipmentType"
                                required="true"/>
                            <myEquipment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="My Equipment Label (i18N key)"
                                name="./myEquipment"
                                required="true"/>
                            <applyFilter
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Apply Filter Label (i18N key)"
                                name="./applyFilter"
                                required="true"/>
                            <removeAll
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Remove all Label (i18N key)"
                                name="./removeAll"
                                required="true"/>
                            <first
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="First Label (i18N key)"
                                name="./first"
                                required="true"/>
                            <last
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Last Label (i18N key)"
                                name="./last"
                                required="true"/>
							<hideAndShowCta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Hide And Show Cta Label (i18N key)"
                                name="./hideAndShowCta"
                                required="true"/>
                            <customizeTable
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Customize Table Label (i18N key)"
                                name="./customizeTable"
                                required="true"/>
                            <functionalLocation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Functional Location (i18N key)"
                                name="./functionalLocation"
                                required="true"/>
                            <siteDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Description (i18N key)"
                                name="./siteDescription"
                                required="true"/>
                            <location
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Location (i18N key)"
                                name="./location"
                                required="true"/>
                            <noDataFound
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="No Data Found Label (i18N key)"
                                    name="./noDataFound"
                                    required="true"/>
                            <permanentVolumeConversion
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Permanent Volume Conversion Label (i18N key)"
                                    name="./permanentVolumeConversion"
                                    required="true"/>
							<showallfilters
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Show all filters (i18N key)"
                                    name="./showAllFilters"
                                    required="true"/>
							<hideallfilters
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Hide all filters (i18N key)"
                                    name="./hideAllFilters"
                                    required="true"/>
							<removeallfilters
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Remove all filters (i18N key)"
                                    name="./removeAllFilters"
                                    required="true"/>
                            <addNewEquipmentLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Add new equipment label (i18N key)"
                                    name="./addNewEquipmentLabel"
                                    required="true"/>
                            <addNewEquipmentUrl
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Add new equipment URL (i18N key)"
                                    name="./addNewEquipmentUrl"
                                    rootPath="/content/tetrapak/customerhub"
                                    required="true"/>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tool tips Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <countryToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country Tool Tip Text (i18N key)"
                                name="./countryToolTip"/>
							<siteToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Tool Tip Text (i18N key)"
                                name="./siteToolTip"/>
							<lineToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Line Tool Tip Text (i18N key)"
                                name="./lineToolTip"/>
							<equipDescToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Description Tool Tip Text (i18N key)"
                                name="./equipDescToolTip"/>
							<serialNumToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Serial Number Tool Tip Text (i18N key)"
                                name="./serialNumToolTip"/>
							<equipStatToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Equipment Status Tool Tip Text (i18N key)"
                                name="./equipStatToolTip"/>
                            <functionalLocationToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Functional Location Tool Tip Text (i18N key)"
                                name="./functionalLocationToolTip"/>
                            <siteDescToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Description Tool Tip Text (i18N key)"
                                name="./siteDescToolTip"/>
                            <locationToolTip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Location Tool Tip Text (i18N key)"
                                name="./locationToolTip"/>
                        </items>
                    </column2>
                    <column3
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Path Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <equipmentDetailsPath
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                    fieldLabel="Path To Equipment Details Page"
                                    name="./equipmentDetailsPath"
                                    required="true"/>
                        </items>
                    </column3>
                    <column4
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Filter Limit Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <maxLimitErrorPre
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Filter Limit Reached Pre Text (i18N key)"
                                    required="true"
                                    name="./maxLimitErrorPre"/>
                            <maxLimitErrorAfter
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Filter Limit Reached After Text (i18N key)"
                                    required="true"
                                    name="./maxLimitErrorAfter"/>
                            <filtersOf
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Filters OF Text (i18N key)"
                                    required="true"
                                    name="./filtersOf"/>
                            <filtersSelected
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Filters Selected Text (i18N key)"
                                    required="true"
                                    name="./filtersSelected"/>
                        </items>
                    </column4>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
