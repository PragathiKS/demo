<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Equipment"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <content
                        jcr:primaryType="nt:unstructured"
                        jcr:title="My equipment i18n Keys Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <country
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Country Label"
                                        fieldLabel="Country Label"
                                        name="./country"
                                        required="true"/>
                                    <line
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Line Label"
                                        fieldLabel="Line Label"
                                        name="./line"
                                        required="true"/>
                                    <searchResults
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Search Results Label"
                                        fieldLabel="Search Results Label"
                                        name="./searchResults"
                                        required="true"/>
                                    <site
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Site Label"
                                        fieldLabel="Site Label"
                                        name="./site"
                                        required="true"/>
                                    <equipmentDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Equipment Description Label"
                                        fieldLabel="Equipment Description Label"
                                        name="./equipmentDescription"
                                        required="true"/>
                                    <serialNumber
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Serial Number Label"
                                        fieldLabel="Serial Number Label"
                                        name="./serialNumber"
                                        required="true"/>
                                    <equipmentStatus
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Equipment Status Label"
                                        fieldLabel="Equipment Status Label"
                                        name="./equipmentStatus"
                                        required="true"/>
                                    <myEquipment
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter My Equipment Label"
                                        fieldLabel="My Equipment Label"
                                        name="./myEquipment"
                                        required="true"/>
                                    <applyFilter
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Apply Filter Label"
                                        fieldLabel="Apply Filter Label"
                                        name="./applyFilter"
                                        required="true"/>
                                    <removeAll
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Remove all Label"
                                        fieldLabel="Remove all Label"
                                        name="./removeAll"
                                        required="true"/>
                                </items>
                            </column>
                        </items>
                    </content>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
