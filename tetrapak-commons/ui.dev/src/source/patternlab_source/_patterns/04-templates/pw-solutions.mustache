<div class="tp_layout tp_layout-pw">
    <div class="row">
        <div class="col-sm-12">
            {{# header }}
            {{> organisms_pw-header }}
            {{/ header }}
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            {{# tp_pw_hero }}
            {{> organisms_pw-tp_pw-hero }}
            {{/ tp_pw_hero }}

            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        {{# tp_pw_breadcrumb }}
                        {{> organisms_pw-tp_pw-breadcrumb }}
                        {{/ tp_pw_breadcrumb }}

                        {{# tp_pw_articleBlock }}
                        {{> organisms_pw-tp_pw-articleBlock_v2 }}
                        {{/ tp_pw_articleBlock }}

                        {{# tp_pw_articlesNoImg }}
                        {{> organisms_pw-tp_pw-articlesNoImg }}
                        {{/ tp_pw_articlesNoImg }}

                        {{# tp_pw_imageText }}
                        {{> organisms_pw-tp_pw-imageText-popout }}
                        {{/ tp_pw_imageText }}

                        {{# tp_pw_articleBlockVideo }}
                        {{> organisms_pw-tp_pw-articleBlockVideo }}
                        {{/ tp_pw_articleBlockVideo }}

                        {{# tp_pw_tabContentReverse }}
                            {{> organisms_pw-tp_pw-tabListContent-reverse }}
                        {{/ tp_pw_tabContentReverse }}

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            {{# footer }}
            {{> organisms_pw-footer }}
            {{/ footer }}
        </div>
    </div>
</div>
