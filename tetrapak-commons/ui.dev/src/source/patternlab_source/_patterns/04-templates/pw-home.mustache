<div class="tp_layout tp_layout-pw">
    <div class="row">
        <div class="col-sm-12">
            {{# header }}
            {{> organisms_pw-header }}
            {{/ header }}
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            {{# tp_pw_hero }}
            {{> organisms_pw-tp_pw-heroHome }}
            {{/ tp_pw_hero }}

            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        {{# tp_pw_articleBlock }}
                        {{> organisms_pw-tp_pw-articleBlock }}
                        {{/ tp_pw_articleBlock }}

                        {{# tp_pw_imageTextRight }}
                        {{> organisms_pw-tp_pw-imageTextRight }}
                        {{/ tp_pw_imageTextRight }}

                        {{# tp_pw_tabContent }}
                            {{> organisms_pw-tp_pw-tabListContent }}
                        {{/ tp_pw_tabContent }}

                        {{# tp_pw_tabContentReverse }}
                            {{> organisms_pw-tp_pw-tabListContent-reverse }}
                        {{/ tp_pw_tabContentReverse }}

                        {{# tp_pw_imageText }}
                        {{> organisms_pw-tp_pw-imageText }}
                        {{/ tp_pw_imageText }}

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            {{# footer }}
            {{> organisms_pw-footer }}
            {{/ footer }}
        </div>
    </div>
</div>
