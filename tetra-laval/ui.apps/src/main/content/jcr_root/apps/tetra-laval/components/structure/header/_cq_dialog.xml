<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <general
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <logoimage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Link to Logo image"
                                fieldLabel="Logo Image path"
                                name="./logoImagePath"
                                rootPath="/content/dam/tetra-laval"/>
                            <logolink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Where the logo should direct"
                                fieldLabel="Logo Image link"
                                name="./logoLink"
                                rootPath="/content/tetra-laval"/>
                            <logoalt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Required for Accessibility use"
                                fieldLabel="Logo Image Alt Text"
                                name="./logoAlt"
                                required="true"/>
                            <contactLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Where the contact icon should direct, if empty, contact icon will not appear"
                                fieldLabel="Contact link"
                                name="./contactLink"
                                rootPath="/content/tetra-laval"/>
                            <searchLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Where the search icon should direct, if empty, search icon will not appear"
                                fieldLabel="Search link"
                                name="./searchLink"
                                rootPath="/content/tetra-laval"/>
                        </items>
                    </general>
                </items>
            </general>
        </items>
    </content>
</jcr:root>
