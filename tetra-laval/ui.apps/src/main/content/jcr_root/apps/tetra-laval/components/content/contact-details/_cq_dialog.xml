<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact Details"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <baseInfo
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <contactDetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Contact Details">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Click to Expand"
                                    sling:resourceType="granite/ui/components/foundation/container"
                                    name="./contactDetails">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/collapsible"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <details
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/apps/publicweb/components/content/richtext/cq:dialog/content/items/rte/items/column/items/text"/>
                                        <anchorId
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/apps/publicweb/components/content/anchorSelector/cq:dialog/content/items/anchor/items/column/items/anchorId"/>
                                        <anchorTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                            path="/apps/publicweb/components/content/anchorSelector/cq:dialog/content/items/anchor/items/column/items/anchorTitle"/>
                                    </items>
                                </field>
                            </contactDetails>
                            <mapUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Map url"
                                name="./mapUrl"/>
                        </items>
                    </column>
                </items>
            </baseInfo>
            <spacing
                jcr:primaryType="nt:unstructured"
                jcr:title="Spacing"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <padding
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="/apps/publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/padding"/>
                            <topmargin
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="/apps/publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/topmargin"/>
                        </items>
                    </column>
                </items>
            </spacing>
        </items>
    </content>
</jcr:root>