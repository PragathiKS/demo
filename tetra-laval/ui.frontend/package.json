{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "test": "karma start karma.conf.js --color",
    "eslint": "eslint \"./src/main/webpack/components/**/*.js\" \"./src/main/webpack/source/**/*.js\" --color",
    "eslint:fix": "eslint --fix \"./src/main/webpack/components/**/*.js\" \"./src/main/webpack/source/**/*.js\" --color",
    "sasslint": "sass-lint --config ./sass-lint.yml ./src/main/webpack/components/**/*.scss -v -q",
    "sasslint:report": "sass-lint --config './sass-lint.yml' ./src/main/webpack/components/**/*.scss -v -q -f html -o ./sass-lint-report.html",
    "testProd": "karma start karma.conf.js --mode=production --color",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env.writeToDisk & watch 'clientlib' ./dist & aemsync -w ../ui.apps/src/main/content"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.4.3",
    "aemsync": "^4.0.1",
    "autoprefixer": "^9.2.1",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.2.1",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-sinon": "^4.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-sinon": "^0.2.0",
    "file-loader": "^6.2.0",
    "glob-import-loader": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "karma": "^6.3.4",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.17.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.4.0",
    "source-map-loader": "^0.2.4",
    "string-replace-loader": "^2.3.0",
    "style-loader": "^0.14.1",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.3.3333",
    "url-loader": "^4.1.1",
    "watch": "^1.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.1",
    "yargs": "^15.0.1"
  },
  "dependencies": {},
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
