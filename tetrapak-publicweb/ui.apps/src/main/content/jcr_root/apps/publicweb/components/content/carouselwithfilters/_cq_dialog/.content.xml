<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Public Web Carousel With Filters"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items
            jcr:primaryType="nt:unstructured"
            sling:hideChildren="[column]">
            <properties
                jcr:primaryType="nt:unstructured"
                jcr:title="Properties"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter title"
                                fieldLabel="Title"
                                name="./titleI18n"/>
                            <titlealignment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                defaultValue="left"
                                fieldDescription="Set the title alignment"
                                fieldLabel="Title Alignment"
                                name="./titleAlignment">
                                <items jcr:primaryType="nt:unstructured">
                                    <left
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="Left"
                                        value="text-left"/>
                                    <center
                                        jcr:primaryType="nt:unstructured"
                                        text="Center"
                                        value="text-center"/>
                                    <right
                                        jcr:primaryType="nt:unstructured"
                                        text="Right"
                                        value="text-right"/>
                                </items>
                            </titlealignment>
                            <tabdetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Tabs and details"
                                class="full-width">
                                    <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                        acs-commons-nested="NODE_STORE"
                                        name="./tabDetails">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                            method="absolute"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <contentType
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Select Content Type"
                                                        name="./contentType">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <automatic
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Automatic"
                                                                value="automatic"/>
                                                            <manual
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Manual"
                                                                value="manual"/>
                                                        </items>
                                                    </contentType>
                                                    <articlepath
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Select practice line path to automatically populate values when content type is automatic"
                                                        fieldLabel="Practice Line Path"
                                                        name="./practicePath"
                                                        rootPath="/content"/>
                                                    <title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Practice Line Title"
                                                        name="./practiceTitle"/>
                                                    <vanityDescription
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldLabel="Description"
                                                        name="./vanityDescription"/>
                                                    <linkText
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Link Text"
                                                        name="./ctaTexti18nKey"/>
                                                    <linkTarget
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Link Target"
                                                        name="./linkTarget">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <new
                                                                jcr:primaryType="nt:unstructured"
                                                                text="New Window"
                                                                value="_blank"/>
                                                            <same
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Same Window"
                                                                value="_self"/>
                                                        </items>
                                                    </linkTarget>
                                                    <imagepath
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Select Practice Line Image Path"
                                                        fieldLabel="Practice Line Image Path"
                                                        name="./practiceImagePath"
                                                        rootPath="/content/dam/publicweb"/>
                                                    <imagealt
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter Alt Text"
                                                        fieldLabel="Alt Text"
                                                        name="./practiceImageAltI18n"/>
                                                </items>
                                            </column>
                                        </items>
                                    </field>
                            </tabdetails>
                        </items>
                    </column>
                </items>
            </properties>
            <design
                jcr:primaryType="nt:unstructured"
                jcr:title="Design"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <pwThemes
                                jcr:primaryType="nt:unstructured"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/pwThemes"
                                sling:resourceType="granite/ui/components/foundation/include">
                            </pwThemes>
                            <pwButtonThemes
                                jcr:primaryType="nt:unstructured"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/pwButtonThemes"
                                sling:resourceType="granite/ui/components/foundation/include">
                            </pwButtonThemes>
                            <padding
                                jcr:primaryType="nt:unstructured"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/padding"
                                sling:resourceType="granite/ui/components/foundation/include">
                            </padding>
                        </items>
                    </column>
                </items>
            </design>
        </items>
    </content>
</jcr:root>