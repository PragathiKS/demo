<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="PublicWeb Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="pw-footer"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <logoimagepath
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldDescription="Select Image Path"
                            fieldLabel="Logo Image Path"
                            name="./logoImagePath"
                            rootPath="/content/dam/publicweb"/>
                    <alttext
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter Alt Text i18 Key"
                            fieldLabel="Alt Text i18 Key"
                            name="./altTextI18n"/>
                    <logoimagelink
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldDescription="Select path of the page that links to the logo."
                            fieldLabel="Logo Image Link"
                            name="./logoImageLink"
                            rootPath="/content/publicweb"/>
                    <textlinks
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Text Links"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="List of links">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./textlinks">
                            <items jcr:primaryType="nt:unstructured">
                                <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter title i18 key"
                                            fieldLabel="Title i18 Key"
                                            name="./titleI18n"/>
                                        <pagepath
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Select Link"
                                            fieldLabel="Link Path"
                                            name="./pagepath"
                                            rootPath="/content/publicweb"/>
                                        <newwindow
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                            fieldDescription="Open link in a new window ?"
                                            fieldLabel="New Window ?"
                                            name="./newwindow"/>
                                    </items>
                                </column>
                            </items>
                        </field>
                    </textlinks>
                    <sociallinks
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Social Links"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="List of links">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./sociallinks">
                            <items jcr:primaryType="nt:unstructured">
                                <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <iconpath
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Select Icon Path"
                                            fieldLabel="Icon Path"
                                            name="./iconpath"
                                            rootPath="/content/dam/publicweb"/>
                                        <alttext
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter alt text  i18 Key for the icon"
                                            fieldLabel="Icon alt text  i18 Key"
                                            name="./iconAltI18n"/>
                                        <iconPath
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Select Icon Link"
                                            fieldLabel="Icon Link"
                                            name="./iconPath"
                                            rootPath="/content/publicweb"/>
                                        <newwindow
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                            fieldDescription="Open link in a new window ?"
                                            fieldLabel="New Window ?"
                                            name="./newwindow"/>
                                    </items>
                                </column>
                            </items>
                        </field>
                    </sociallinks>
                    <anchorlabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Enter To Top anchor label i18 key"
                        fieldLabel="To Top Anchor Label i18 Key"
                        name="./anchorlabelI18n"/>
                    <anchorlink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldDescription="Select To Top Anchor Link"
                        fieldLabel="To Top Anchor Link Path"
                        name="./anchorlink"
                        rootPath="/content/publicweb"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
