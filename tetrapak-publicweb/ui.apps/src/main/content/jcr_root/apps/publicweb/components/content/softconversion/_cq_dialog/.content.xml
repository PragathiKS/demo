<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact us"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <softconversionconfig
                jcr:primaryType="nt:unstructured"
                jcr:title="Soft conversion form"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <downloadready
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Download Ready screen"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                        fieldLabel="Heading"
                                        value="Your download is ready"
                                        name="./downloadReadyHeading"/>
                                    <summarytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 220 chars considering translations"
                                        fieldLabel="Body Text"
                                        name="./downloadReadyDescriptionText"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="modifylink,unlink"/>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="-"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="-"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <dialogFullScreen
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,links#modifylink,links#unlink]"/>
                                            </cui>
                                        </uiSettings>
                                    </summarytext>
                                </items>
                            </downloadready>
                            <welcomeback
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Welcome Back screen"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                        fieldLabel="Heading"
                                        value="Welcome back"
                                        name="./welcomeBackHeading"/>
                                    <summarytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 220 chars considering translations"
                                        fieldLabel="Body Text"
                                        name="./welcomeBackDescriptionText"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="modifylink,unlink"/>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="-"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="-"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <dialogFullScreen
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,links#modifylink,links#unlink]"/>
                                            </cui>
                                        </uiSettings>
                                    </summarytext>
                                    <yesbuttonlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                        fieldLabel="Yes button label"
                                        value="Yes, this is me"
                                        name="./yesButtonLabel"/>
                                    <nobuttonlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                        fieldLabel="No button label"
                                        value="No, this is not me"
                                        name="./noButtonLabel"/>
                                </items>
                            </welcomeback>
                            <thankyou
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Thank you section"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                        fieldLabel="Heading"
                                        value="Thank you for your download"
                                        name="./thankyouHeading"/>
                                    <summarytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 220 chars considering translations"
                                        fieldLabel="Body Text"
                                        name="./thankyouDescriptionText"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="modifylink,unlink"/>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="-"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="-"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <dialogFullScreen
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,links#modifylink,links#unlink]"/>
                                            </cui>
                                        </uiSettings>
                                    </summarytext>
                                </items>
                            </thankyou>
							<positiontags
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Positions displayed based on tag selected. Select 'Position' tag."
                                fieldLabel="Position Tags Path"
                                name="./positionTagsPath"
                                rootPath="/content/cq:tags/pardot-system-config"/>
							<functiontags
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Functions displayed based on tag selected. Select 'Functions' tag."
                                fieldLabel="Functions Tags Path"
                                name="./functionTagsPath"
                                rootPath="/content/cq:tags/pardot-system-config"/>	
                        </items>
                    </column>
                </items>
            </softconversionconfig>
            <form
                jcr:primaryType="nt:unstructured"
                jcr:title="Soft conversion form"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                fieldLabel="Heading"
                                name="./heading"/>
                            <apiurl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Provide pardot form handler URL. Valid URL starts with - https://go.tetrapak.com/"
                                fieldLabel="Pardot form handler URL"
                                name="./pardotUrl"/>                             
                            <summarytext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldDescription="Suggested to use characters less than recommended Limit: 220 chars considering translations"
                                fieldLabel="Body Text"
                                name="./descriptionText"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="modifylink,unlink"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="-"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <dialogFullScreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,links#modifylink,links#unlink]"/>
                                    </cui>
                                </uiSettings>
                            </summarytext>
                            <thankyou
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Thank you section"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <morebuttonlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Suggested to use characters less than recommended Limit: 40 chars considering translations"
                                        fieldLabel="More button label"
                                        value="More white papers"
                                        name="./moreButtonLabel"/>
                                    <morebuttonaction
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Provide Link for More Whitepapers button"
                                        fieldLabel="More button link/action"
                                        name="./moreButtonAction"
                                        rootPath="/content/tetrapak/publicweb"/>
                                </items>
                            </thankyou>
                        </items>
                    </column>
                </items>
            </form>
        </items>
    </content>
</jcr:root>
