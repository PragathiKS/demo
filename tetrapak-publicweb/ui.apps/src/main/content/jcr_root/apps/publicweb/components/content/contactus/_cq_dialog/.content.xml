<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact us"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <contactus
                jcr:primaryType="nt:unstructured"
                jcr:title="Contact us"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Heading"
                                name="./heading"/>
                            <summarytext
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Description"
                               name="./descriptionText"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="modifylink,unlink"/>
                                   <paraformat
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <dialogFullScreen
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[links#modifylink,links#unlink,#paraformat]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <paraformat
                                                   jcr:primaryType="nt:unstructured"
                                                   items="paraformat:getFormats:paraformat-pulldown"
                                                   ref="paraformat"/>
                                           </popovers>
                                       </dialogFullScreen>
                                   </cui>
                               </uiSettings>
                           </summarytext>
                            <file
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                allowUpload="{Boolean}false"
                                autoStart="{Boolean}false"
                                class="cq-droptarget"
                                fieldLabel="Image"
                                fileNameParameter="./fileName"
                                fileReferenceParameter="./image"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./file"
                                title="Upload Image Asset"
                                uploadUrl="${suffix.path}"
                                useHTML5="{Boolean}true"/>
                            <alt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                fieldLabel="Alternative Text"
                                name="./alt"/>
                        </items>
                    </column>
                </items>
            </contactus>
            <form
                jcr:primaryType="nt:unstructured"
                jcr:title="Form"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step1
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Form Step 1"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <purposeofcontacttext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Step 1 headline"
                                        name="./purposeOfContactText"/>
                                </items>
                            </step1>
                            <step2
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Form Step 2"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <contactinfotext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Step 2 headline"
                                        name="./contactInfoText"/>
                                    <summarytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Summary heading text"
                                        name="./summaryText"/>
                                </items>
                            </step2>
                            <step3
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Form Step 3"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <howcanwehelptext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Step 3 headline"
                                        name="./howCanWeHelpText"/>
                                    <privacypolicytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Privacy policy text"
                                        name="./privacyPolicy"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="modifylink,unlink"/>
                                            <paraformat
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <dialogFullScreen
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[links#modifylink,links#unlink,#paraformat]">
                                                    <popovers jcr:primaryType="nt:unstructured">
                                                        <paraformat
                                                            jcr:primaryType="nt:unstructured"
                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                            ref="paraformat"/>
                                                    </popovers>
                                                </dialogFullScreen>
                                            </cui>
                                        </uiSettings>
                                    </privacypolicytext>
                                </items>
                            </step3>
                        </items>
                    </column>
                </items>
            </form>
            <thankyou
                jcr:primaryType="nt:unstructured"
                jcr:title="Thank you"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Heading"
                                name="./thankyouHeading"/>
                            <file
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                allowUpload="{Boolean}false"
                                autoStart="{Boolean}false"
                                class="cq-droptarget"
                                fieldLabel="Thank you Image"
                                fileNameParameter="./fileName"
                                fileReferenceParameter="./thankyouImage"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./file"
                                title="Upload Thank you Image"
                                uploadUrl="${suffix.path}"
                                useHTML5="{Boolean}true"/>
                            <alt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                fieldLabel="Alternative Text"
                                name="./thankyouImageAltText"/>
                            <summarytext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Description"
                                name="./thankyouDescriptionText"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="modifylink,unlink"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <dialogFullScreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </dialogFullScreen>
                                    </cui>
                                </uiSettings>
                            </summarytext>
                        </items>
                    </column>
                </items>
            </thankyou>
            <anchor
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/publicweb/components/content/anchorSelector/cq:dialog/content/items/anchor"/>
            <design
                jcr:primaryType="nt:unstructured"
                jcr:title="Design"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <pwThemes
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/pwThemes"/>
                        </items>
                    </column>
                </items>
            </design>
        </items>
    </content>
</jcr:root>
