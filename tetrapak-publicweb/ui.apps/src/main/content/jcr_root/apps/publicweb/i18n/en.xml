<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:language="en"
    jcr:mixinTypes="[mix:language]"
    jcr:primaryType="nt:folder">
    <country
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="country"
        sling:message="Country"/>
    <office
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="office"
        sling:message="Office"/>
    <pxpopenings jcr:primaryType="nt:folder">
        <benefits
            jcr:primaryType="sling:MessageEntry"
            sling:key="openingbenefits"
            sling:message="Benefits:"/>
        <principle
            jcr:primaryType="sling:MessageEntry"
            sling:key="openingprinciple"
            sling:message="Principle:"/>
        <type
            jcr:primaryType="sling:MessageEntry"
            sling:key="openingtype"
            sling:message="Type:"/>
    </pxpopenings>
    <selectCountry
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="selectCountry"
        sling:message="Select Country"/>       
    <selectOffice
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="selectOffice"
        sling:message="Select Office"/>
   <goToLocalSite
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="goToLocalSite"
        sling:message="Go to Local Site"/>
   <viewLargerMap
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="viewLargerMap"
        sling:message="View larger map"/>
   <previous
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="previous"
        sling:message="Previous"/>
    <next
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="next"
        sling:message="Next"/>       
    <submit
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="submit"
        sling:message="Submit"/>
   <newrequest
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="newrequest"
        sling:message="New Request"/>
   <message
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="message"
        sling:message="Message"/>
   <firstName
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="firstName"
        sling:message="First name"/>
    <lastName
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="lastName"
        sling:message="Last name"/>
   <email
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="email"
        sling:message="E-mail"/>
   <emailPlaceholder
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="emailPlaceholder"
        sling:message="E.g. example@example.com"/>
   <sustainability
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="sustainability"
        sling:message="Sustainability"/>
   <career
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="career"
        sling:message="Career"/>
   <other
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="other"
        sling:message="Other"/>
   <pleaseselectanoption
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="pleaseselectanoption"
        sling:message="Please select an option"/>
   <pleaseselect
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="pleaseselect"
        sling:message="Please select"/>  
</jcr:root>
