<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:language="en"
    jcr:mixinTypes="[mix:language]"
    jcr:primaryType="nt:folder">
    <country
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="country"
        sling:message="Country"/>
    <office
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="office"
        sling:message="Office"/>
    <pxpopenings jcr:primaryType="nt:folder">
        <benefits
            jcr:primaryType="sling:MessageEntry"
            sling:key="openingbenefits"
            sling:message="Benefits:"/>
        <principle
            jcr:primaryType="sling:MessageEntry"
            sling:key="openingprinciple"
            sling:message="Principle:"/>
        <type
            jcr:primaryType="sling:MessageEntry"
            sling:key="openingtype"
            sling:message="Type:"/>
    </pxpopenings>
    <selectCountry
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="selectCountry"
        sling:message="Select Country"/>       
    <selectOffice
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="selectOffice"
        sling:message="Select Office"/>
   <goToLocalSite
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="goToLocalSite"
        sling:message="Go to Local Site"/>
   <viewLargerMap
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="sling:MessageEntry"
        sling:key="viewLargerMap"
        sling:message="View larger map"/>
</jcr:root>
