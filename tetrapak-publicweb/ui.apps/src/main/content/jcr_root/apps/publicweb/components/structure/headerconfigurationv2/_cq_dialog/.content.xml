<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Configurations"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <general
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <logoimage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Link to Logo image"
                                fieldLabel="Logo Image path"
                                name="./logoImagePath"
                                rootPath="/content/dam/tetrapak"/>
                            <logolink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Where the logo should direct"
                                fieldLabel="Logo Image link"
                                name="./logoLink"
                                rootPath="/content/tetrapak/publicweb"/>
                            <logoalt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Required for Accessibility use"
                                fieldLabel="Logo Image Alt Text"
                                name="./logoAlt"
                                required="true"/>
                            <loginlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="My TetraPak Label"
                                name="./loginLabel"/>
                            <loginlink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldDescription="Link to MyTetraPak Login screen "
                                fieldLabel="My TetraPak Link"
                                name="./loginLink"
                                rootPath="/content/tetrapak/publicweb"/>
                            <marketSelectorDisabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    fieldDescription="Select to Disable market selector"
                                    fieldLabel="Disable market selector"
                                    name="./marketSelectorDisabled"
                                    text="Disable market selector"
                                    uncheckedValue="{Boolean}false"
                                    value="{Boolean}true"/>
                            <marketTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Market Selector Title"
                                    name="./marketTitle"/>
                            <secondarynavigationlinks
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                    composite="{Boolean}true"
                                    fieldLabel="Secondary Navigation Links">
                                <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        name="./secondaryNavigationLinks">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <linkText
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Link text"
                                                        fieldLabel="Link label"
                                                        name="./linkText"/>
                                                <linkUrl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                        fieldDescription="Link to destination path"
                                                        fieldLabel="Link path"
                                                        name="./linkUrl"
                                                        rootPath="/content/tetrapak/publicweb"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </secondarynavigationlinks>
                            <mainnavigationlinks
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                    composite="{Boolean}true"
                                    fieldLabel="Main Navigation Links">
                                <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        name="./mainNavigationLinks">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <linkText
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Link text"
                                                        fieldLabel="Link label"
                                                        name="./linkText"/>
                                                <megamenupath
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/experience-fragments/editor/components/xffield"
                                                        granite:id="xfPicker"
                                                        name="./megaMenuPath"
                                                        fieldLabel="Path to Mega Menu Experience Fragment"
                                                        filter="folderOrVariant"
                                                        propertyFilter="cq:xfShowInEditor"
                                                        variant="web"
                                                        fieldDescription="Choose the mega menu experience fragment variation"/>
                                                <linkUrl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                        fieldDescription="Link to destination path"
                                                        fieldLabel="Link path"
                                                        name="./linkUrl"
                                                        rootPath="/content/tetrapak/publicweb"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </mainnavigationlinks>
                            <searchpage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldLabel="Search page path"
                                name="./searchPage"
                                rootPath="/content/tetrapak/publicweb"/>
                        </items>
                    </column>
                </items>
            </general>
        </items>
    </content>
</jcr:root>
