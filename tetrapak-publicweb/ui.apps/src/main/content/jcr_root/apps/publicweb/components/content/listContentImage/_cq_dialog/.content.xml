<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Public Web List Content Image"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items
            jcr:primaryType="nt:unstructured"
            sling:hideChildren="[column]">
            <properties
                jcr:primaryType="nt:unstructured"
                jcr:title="Properties"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter the title"
                                fieldLabel="Title"
                                name="./titleI18n"/>
                            <tabs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="Click 'Add field' to add a new tab"
                                maxlinksallowed="10"
                                fieldLabel="Tabs">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested=""
                                    name="./tabLinks">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <linktext
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Link Text for the tab"
                                                    fieldLabel="Tab Link Text"
                                                    name="./tabLinkTextI18n"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </tabs>
                            <linktext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Link Text"
                                fieldLabel="Link Text"
                                name="./linkTextI18n"/>
                            <linktooltip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Link Tool Tip"
                                fieldLabel="Link Tool Tip"
                                name="./linkTooltipI18n"/>
                            <linkpath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Select Link"
                                fieldLabel="Link Path"
                                name="./linkPath"
                                rootPath="/content"/>
                            <targetBlank
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Open link in a new window ?"
                                text="New Window"
                                value="true"
                                name="./targetBlank"/>
                        </items>
                    </column>
                </items>
            </properties>
            <design
                jcr:primaryType="nt:unstructured"
                jcr:title="Design"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <pwThemes
                                jcr:primaryType="nt:unstructured"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/pwThemes"
                                sling:resourceType="granite/ui/components/foundation/include">
                            </pwThemes>
                            <pwButtonThemes
                                jcr:primaryType="nt:unstructured"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/pwButtonThemes"
                                sling:resourceType="granite/ui/components/foundation/include">
                            </pwButtonThemes>
                            <padding
                                jcr:primaryType="nt:unstructured"
                                path="publicweb/components/content/themeSelector/cq:dialog/content/items/design/items/column/items/padding"
                                sling:resourceType="granite/ui/components/foundation/include">
                            </padding>
                            <tabsalignment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                defaultValue="left"
                                fieldDescription="Set the tabs alignment"
                                fieldLabel="Layout Display"
                                name="./tabsAlignment">
                                <items jcr:primaryType="nt:unstructured">
                                    <left
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="Row - Left to right"
                                        value="text-left"/>
                                    <right
                                        jcr:primaryType="nt:unstructured"
                                        text="Row - Right to Left"
                                        value="text-right"/>
                                </items>
                            </tabsalignment>
                        </items>
                    </column>
                </items>
            </design>
        </items>
    </content>
</jcr:root>