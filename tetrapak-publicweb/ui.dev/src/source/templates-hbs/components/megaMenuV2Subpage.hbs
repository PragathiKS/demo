{{#if col1}}
{{#if_eq col1.bgColor 'grey'}}
<div class="tp-pw-megamenucolumn grey-bg">
{{else}}
<div class="tp-pw-megamenucolumn white-bg">
{{/if_eq}}
	{{#if col1.columnItems.heading}}
        <div class="tp-pw-megamenuheading">
            {{#if col1.columnItems.heading.headingURL}}
                <a href="{{col1.columnItems.heading.headingURL}}"><h2>{{col1.columnItems.heading.heading}}</h2></a>
            {{else}}
                <h2>{{col1.columnItems.heading.heading}}</h2>
            {{/if}}    
        </div>
        {{#if col1.columnItems.description}}
        	<div class="tp-pw-megamenudescription">{{{col1.columnItems.description.text}}}</div>
        {{/if}}
        {{#if col1.columnItems.navigationlinks.navLinks}}
            <div class="tp-pw-megamenunavigationlinks">
                {{#each col1.columnItems.navigationlinks.navLinks}}
                    {{#if linkLabel}}
                        <div class="tp-pw-megamenunavigationlinks__item nav-link-bottom-border navLink{{@index}}">
                            {{#if linkIconClass}}
                            <i class="{{linkIconClass}}"></i>    
                            {{/if}}
                            <a href="{{linkUrl}}">{{linkLabel}}</a>
                        </div>
                    {{/if}}
                {{/each}}
            </div>
		{{/if}}
	{{/if}}
	{{#if col1.columnItems.megaMenuAccordions}}
		{{#each col1.columnItems.megaMenuAccordions}}
        	<div class="accordion" id="accordion-col1-{{@index}}">
			<div class="tp-pw-megamenusubheading">
                <div class="tp-pw-accordion-header" data-toggle="collapse" data-target="#collapse-col1-{{@index}}" aria-expanded="true" aria-controls="collapse-col1-{{@index}}">
                    <h3>{{subHeadings.subheading.subheading}}</h3>
                    <i class="icon icon-Navigation_Right_pw"></i>
                </div>
            </div>
			<div id="collapse-col1-{{@index}}" class="collapse show" aria-labelledby="heading-col1-{{@index}}" data-parent="#accordion-col1-{{@index}}">
			{{#if subHeadings.description}}
					<div class="tp-pw-megamenudescription">{{{subHeadings.description.text}}}</div>
			{{/if}}
            {{#if subHeadings.subheading.subheadingOverviewText}}
                <div class="tp-pw-megamenusubheading-overviewlink">
            	    <a href="{{subHeadings.subheading.subheadingURL}}">{{subHeadings.subheading.subheadingOverviewText}}</a>
                    <i class="icon icon-Navigation_Right_pw"></i>
                </div>
            {{/if}}
			{{#if subHeadings.navigationlinks.navLinks}}
                <div class="tp-pw-megamenusubheading-separator"></div>
                <div class="tp-pw-megamenunavigationlinks">
                    {{#each subHeadings.navigationlinks.navLinks}}
                        {{#if linkLabel}}
                        <div class="tp-pw-megamenunavigationlinks__item nav-link-bottom-border navLink{{@index}}">
                            {{#if linkIconClass}}
                            <i class="{{linkIconClass}}"></i>    
                            {{/if}}    
                            <a href="{{linkUrl}}">{{linkLabel}}</a>
                        </div>
                        {{/if}}
                    {{/each}}
                </div>
			{{/if}}
			</div>
            </div>
		{{/each}}
	{{/if}}
</div>    
{{/if}}
