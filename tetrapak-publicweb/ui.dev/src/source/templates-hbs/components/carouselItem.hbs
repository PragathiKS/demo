<div class="pw-carousel__tabContent tab-content">
  <div class="pw-carousel__tabPane tab-pane show active">
    <div class="pw-carousel__tabPane__img d-flex">
      <img src="{{this.practiceImagePath}}" alt="{{this.practiceImageAltI18n}}">
    </div>
    <div class="pw-carousel__tabPane__text">
      <h3>{{this.practiceTitle}}</h3>
      <p>{{this.vanityDescription}}</p>
      {{> atoms/button-text this}}
    </div>
  </div>
</div>
