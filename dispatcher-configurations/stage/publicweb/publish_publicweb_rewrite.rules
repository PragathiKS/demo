## This ruleset forces https in the end users browser
RewriteCond %{HTTP:X-Forwarded-Proto} !https
RewriteCond %{REQUEST_URI} !^/dispatcher/invalidate.cache
RewriteRule (.*) https://%{SERVER_NAME}%{REQUEST_URI} [L,R=301]

## This ruleset is use for lang-masters based dispatcher URL redirection

RewriteCond   %{HTTP_HOST}  'tetrapak-stage64a.adobecqms.net'
RewriteCond   %{REQUEST_URI} ^/
RewriteRule   "^/$"  "/content/tetrapak/publicweb/lang-masters/en/home.html" [R]
RewriteRule   "^/robots.txt"  "/content/tetrapak/publicweb/robots.txt" [PT]
RewriteRule   "^/sitemap.xml"  "/content/tetrapak/publicweb/sitemap.xml" [PT]
RewriteRule   "^/(.*)/(.*)/sitemap.xml"  "/$1/$2.sitemap.xml" [PT]

RewriteCond   %{REQUEST_URI} !^/.*/jcr:content/.*$
RewriteCond   "%{REQUEST_URI}" "!\.(jpg|gif|png|css|js|xls|xlsx|doc|docx|pdf|jpeg|mov|avi|mp4|json|css|ico|woff|ttf)$"
RewriteCond   %{HTTP_HOST}  'tetrapak-stage64a.adobecqms.net'
RewriteRule   "^/(.*)$"  "/$1.html" [PT,L]

