RewriteRule ^(.*)/$ https://%{HTTP_HOST}$1 [R=301,L]

# Rewrite rules
RewriteMap map.legacy dbm:/etc/httpd/conf.d/rewrites/rewritemap.dbm
RewriteCond ${map.legacy:$1} !=""
RewriteRule ^(.*)$	${map.legacy:$1|/} [L,R=301]


