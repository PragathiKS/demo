## This ruleset forces https in the end users browser
RewriteCond %{HTTP:X-Forwarded-Proto} !https
RewriteCond %{REQUEST_URI} !^/dispatcher/invalidate.cache
RewriteRule (.*) https://%{SERVER_NAME}%{REQUEST_URI} [L,R=301]

RewriteCond   %{HTTP_HOST}  ${PUBLISH_DEFAULT_HOSTNAME}
RewriteCond   %{REQUEST_URI} ^/
RewriteRule   "^/$"  "/content/tetrapak/publicweb/global/en/home.html" [PT,L]

RewriteCond   %{HTTP_HOST} ${PUBLISH_DEFAULT_HOSTNAME}
RewriteRule   "^/online-help$" https://${ONLINEHELP_URL} [R=301,L]

RewriteCond   %{HTTP_HOST} ${PUBLISH_DEFAULT_HOSTNAME}
RewriteRule   "^/online-help/(.*)$" https://${ONLINEHELP_URL}/$1 [R=301,L]


## This ruleset forces www in the end users browser
