<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
	<filter
		root="/conf/global/settings/dam/adminui-extension/metadataschema/training-services"
		mode="update" />
	<filter root="/conf/trs" mode="update" />
	<filter root="/conf/global/settings/workflow/models"
		mode="update" />
	<!-- <filter root="/conf/global/settings/workflow" mode="merge"/> -->
	<filter root="/var/workflow/models/trs-authors" mode="merge" />
	<filter root="/content/trs" mode="update">
		<exclude pattern=".*"/>
        <include pattern=".*/jcr:content.*"/>
        <include pattern=".*/training-videos"/>
        <include pattern=".*/training-videos/jcr:content.*"/>
	</filter>
	<filter root="/content/dam/training-services" mode="update">
        <exclude pattern=".*"/>
        <include pattern=".*/jcr:content.*"/>
	</filter>
</workspaceFilter>
