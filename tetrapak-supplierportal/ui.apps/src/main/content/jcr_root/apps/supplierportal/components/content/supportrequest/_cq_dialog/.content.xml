<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Support Request Form"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="General title (i18N key)"
                                name="./generalTitleLabel"
                                required="true"/>
                            <generalSubtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="General Subtitle (i18N key)"
                                name="./generalSubtitleLabel"
                                required="true"/>
                            <purposeOfContactLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Purpose of Contact (i18N key)"
                                name="./purposeOfContactLabel"
                                required="true"/>
                            <purposeErrorMsgLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Purpose error message (i18N key)"
                                name="./purposeErrorMsgLabel"
                                required="true"/>
                            <queryTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Query Title (i18N key)"
                                name="./queryTitle"
                                required="true"/>
                            <queryPlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Query Subtitle (i18N key)"
                                name="./queryPlaceholder"
                                required="true"/>
                            <querySubtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Query Subtitle (i18N key)"
                                name="./querySubtitle"
                                required="true"/>
                            <queryErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Query Error Message (i18N key)"
                                name="./queryErrorMsgLabel"
                                required="true"/>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Drag and drop"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fileUpdloadErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="File Upload error message (i18N key)"
                                name="./fileErrorMsg"
                                required="true"/>
                            <dragAndDropButtonLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Drag and drop - button label (i18N key)"
                                name="./dragAndDropButtonLabel"
                                required="true"/>
                            <dragAndDropRemoveFileLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Drag and drop - remove file label (i18N key)"
                                name="./dragAndDropRemoveFileLabel"
                                required="true"/>
                        </items>
                    </column2>
                    <column3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Personal Details form"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <detailsTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Details title (i18N key)"
                                name="./detailsTitle"
                                required="true"/>
                            <nameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name Label (i18N key)"
                                name="./nameLabel"
                                required="true"/>
                            <emailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Email Address label (i18N key)"
                                name="./emailLabel"
                                required="true"/>
                            <companyErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="company error message (i18N key)"
                                name="./companyErrorMsg"
                                required="true"/>
                            <companyLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Company Label"
                                name="./companyLabel"
                                required="true"/>
                            <countryLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country label (i18N key)"
                                name="./countryLabel"
                                required="true"/>
                            <countryErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country error message (i18N key)"
                                name="./countryErrorMsg"
                                required="true"/>
                            <cityLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="City Label (i18N key)"
                                name="./cityLabel"
                                required="true"/>
                            <cityErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="City error message (i18N key)"
                                name="./cityErrorMsg"
                                required="true"/>
                            <phoneLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Phone Number Label (i18N key)"
                                name="./phoneLabel"
                                required="true"/>
                            <phoneErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Phone error message (i18N key)"
                                name="./phoneErrorMsg"
                                required="true"/>
                        </items>
                    </column3>
                    <column4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Other Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <otherTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Other title (i18N key)"
                                name="./otherTitle"
                                required="true"/>
                            <descLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Other description Label (i18N key)"
                                name="./otherDescriptionLabel"
                                required="true"/>
                            <aribaNetworkLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Ariba Network label (i18N key)"
                                name="./aribaNetworkLabel"
                                required="true"/>
                            <aribaEmailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Ariba Email Label"
                                name="./aribaEmailLabel"
                                required="true"/>
                            <aribaEmailErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Ariba Email Error Label"
                                name="./aribaEmailErrorMsg"
                                required="true"/>
                            <tpEmailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Tetrapak Email label (i18N key)"
                                name="./tpEmailLabel"
                                required="true"/>
                            <tpEmailErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Tetrapak Email Error label (i18N key)"
                                name="./tpEmailErrorMsg"
                                required="true"/>
                            <sendButtonLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Send button label (i18N key)"
                                name="./SendButtonLabel"
                                required="true"/>
                        </items>
                    </column4>
                    <column5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Thank you form"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <thankyouTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Thankyou title (i18N key)"
                                name="./thankyouTitleLabel"
                                required="true"/>
                            <thankyouSubtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Thankyou subtitle (i18N key)"
                                name="./thankyouSubtitleLabel"
                                required="true"/>
                            <homeButtonLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Home button label (i18N key)"
                                name="./homeButtonLabel"
                                required="true"/>
                        </items>
                    </column5>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
