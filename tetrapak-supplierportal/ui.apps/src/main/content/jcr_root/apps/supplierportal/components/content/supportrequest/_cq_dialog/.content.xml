<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Support Request Form"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    size="L">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="General"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Heading"
                                    name="./generalTitleLabel"
                                    required="true"/>
                            <generalSubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Description"
                                    name="./generalSubtitleLabel"
                                    required="true"/>
                            <purposeOfContactLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Purpose of Contact (i18N key)"
                                    name="./purposeOfContactLabel"
                                    value="Purpose of contract"
                                    required="true"/>
                            <purposeErrorMsgLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Purpose error message (i18N key)"
                                    name="./purposeErrorMsgLabel"
                                    value="Please select a purpose"
                                    required="true"/>
                            <queryTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Query Title (i18N key)"
                                    name="./queryTitle"
                                    value="How can we help you?"
                                    required="true"/>
                            <queryPlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Query Subtitle (i18N key)"
                                    name="./queryPlaceholder"
                                    value="Describe your query"
                                    required="true"/>
                            <querySubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Query Subtitle (i18N key)"
                                    name="./querySubtitle"
                                    value="Tell us, more in detail, about your query..."
                                    required="true"/>
                            <queryErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Query Error Message (i18N key)"
                                    name="./queryErrorMsgLabel"
                                    value="Please describe your query"
                                    required="true"/>
                        </items>
                    </column1>
                    <column2
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Contact Of Puropose"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <onboardingMaintanance
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="On Boarding Maintanance (i18N key)"
                                    name="./onboardingMaintanance"
                                    value="SAP Ariba - Onboarding and maintenance"
                                    required="true"/>
                            <onboardingMaintananceSubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="On Boarding Maintanance Subtitle(i18N key)"
                                    name="./onboardingMaintananceSubtitle"
                                    value="Get support with registration updating supplier information or similar"
                                    required="true"/>
                            <sourcingContracting
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Sourcing Contracting (i18N key)"
                                    name="./sourcingContracting"
                                    value="SAP Ariba - Sourcing &amp; Contracting"
                                    required="true"/>
                            <sourcingContractingSubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Sourcing Contracting Subtitle (i18N key)"
                                    name="./sourcingContractingSubtitle"
                                    value="Get support with sending and responding to RFI/RFQs participating in auctions, using electronic signature"
                                    required="true"/>
                            <catalogues
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Catalogues (i18N key)"
                                    name="./catalogues"
                                    value="SAP Ariba - Catalogues"
                                    required="true"/>
                            <cataloguesSubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Catalogues Subtitle (i18N key)"
                                    name="./cataloguesSubtitle"
                                    value="Get support with creating or maintaining catalogues"
                                    required="true"/>
                            <otherPurpose
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Other purpose (i18N key)"
                                    name="./otherPurpose"
                                    value="Other purpose"
                                    required="true"/>
                            <otherPurposeSubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Other purpose Subtitle (i18N key)"
                                    name="./otherPurposeSubtitle"
                                    value="Get support with something that is not covered in above purposes"
                                    required="true"/>
                        </items>
                    </column2>
                    <column3
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Drag and drop"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fileUpdloadErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="File Upload error message (i18N key)"
                                    name="./fileErrorMsg"
                                    value="File size cannot exceed 5MB"
                                    required="true"/>
                            <dragAndDropButtonLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Drag and drop - button label (i18N key)"
                                    name="./dragAndDropButtonLabel"
                                    value="Attach file"
                                    required="true"/>
                            <dragAndDropRemoveFileLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Drag and drop - remove file label (i18N key)"
                                    name="./dragAndDropRemoveFileLabel"
                                    value="Remove file"
                                    required="true"/>
                        </items>
                    </column3>
                    <column4
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Personal Details form"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <detailsTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Details title (i18N key)"
                                    name="./detailsTitle"
                                    value="Your contact information"
                                    required="true"/>
                            <nameLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Name Label (i18N key)"
                                    name="./nameLabel"
                                    value="Name"
                                    required="true"/>
                            <emailLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email Address label (i18N key)"
                                    name="./emailLabel"
                                    value="Email address"
                                    required="true"/>
                            <companyErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Company error message (i18N key)"
                                    name="./companyErrorMsg"
                                    value="Please enter your company legal name"
                                    required="true"/>
                            <companyLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Company Label"
                                    name="./companyLabel"
                                    value="Company legal name"
                                    required="true"/>
                            <countryLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Country label (i18N key)"
                                    name="./countryLabel"
                                    value="Country"
                                    required="true"/>
                            <countryErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Country error message (i18N key)"
                                    name="./countryErrorMsg"
                                    value="Please enter your country"
                                    required="true"/>
                            <cityLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="City Label (i18N key)"
                                    name="./cityLabel"
                                    value="City"
                                    required="true"/>
                            <cityErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="City error message (i18N key)"
                                    name="./cityErrorMsg"
                                    value="Please enter your city"
                                    required="true"/>
                            <phoneLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Phone Number Label (i18N key)"
                                    name="./phoneLabel"
                                    value="Phone number"
                                    required="true"/>
                            <phoneErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Phone error message (i18N key)"
                                    name="./phoneErrorMsg"
                                    value="Please enter a valid mobile telephone number"
                                    required="true"/>
                        </items>
                    </column4>
                    <column5
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Other Details"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <otherTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Other title (i18N key)"
                                    name="./otherTitle"
                                    value="Other information"
                                    required="true"/>
                            <descLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Other description Label (i18N key)"
                                    name="./otherDescriptionLabel"
                                    value="When possible, share a little more information. This may help our support team to help you faster."
                                    required="true"/>
                            <aribaNetworkLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Ariba Network label (i18N key)"
                                    name="./aribaNetworkLabel"
                                    value="Ariba Network ID(optional)"
                                    required="true"/>
                            <aribaEmailLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Ariba Email Label"
                                    name="./aribaEmailLabel"
                                    value="Ariba account administrator email(optional)"
                                    required="true"/>
                            <aribaEmailErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Ariba Email Error Label"
                                    name="./aribaEmailErrorMsg"
                                    value="Please enter a valid email"
                                    required="true"/>
                            <tpEmailLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Tetrapak Email label (i18N key)"
                                    name="./tpEmailLabel"
                                    value="Tetra Pak contact email(optional)"
                                    required="true"/>
                            <tpEmailErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Tetrapak Email Error label (i18N key)"
                                    name="./tpEmailErrorMsg"
                                    value="Please enter a valid email"
                                    required="true"/>
                            <sendButtonLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Send button label (i18N key)"
                                    name="./SendButtonLabel"
                                    value="Submit"
                                    required="true"/>
                        </items>
                    </column5>
                    <column6
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Thank you form"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <thankyouTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Thankyou title (i18N key)"
                                    name="./thankyouTitleLabel"
                                    value="Thank you!"
                                    required="true"/>
                            <thankyouSubtitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Thankyou subtitle (i18N key)"
                                    name="./thankyouSubtitleLabel"
                                    value="We will get back to you via email as soon as possible"
                                    required="true"/>
                            <homeButtonLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Home button label (i18N key)"
                                    name="./homeButtonLabel"
                                    value="Go to Home"
                                    required="true"/>
                        </items>
                    </column6>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
