<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="All Payments"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="All Payments i18n Keys Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <invoiceno
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Invoice No (i18N key)"
                                name="./invoiceNo"
                                required="true"/>
                            <invoiceDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Invoice Date (i18N key)"
                                name="./invoiceDate"
                                required="true"/>
                            <status
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Status (i18N key)"
                                name="./status"
                                required="true"/>
                            <company
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Company (i18N key)"
                                name="./company"
                                required="true"/>
                            <companyCode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Company Code (i18N key)"
                                name="./companyCode"
                                required="true"/>
                            <country
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Country (i18N key)"
                                name="./country"
                                required="true"/>
                            <supplier
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Supplier(i18N key)"
                                name="./supplier"
                                required="true"/>
                            <supplierCode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Supplier Code(i18N key)"
                                name="./supplierCode"
                                required="true"/>
                            <withholdingtax
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="With Holding Tax(i18N key)"
                                name="./withHoldingTax"
                                required="true"/>
                            <dueDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Due Date(i18N key)"
                                name="./dueDate"
                                required="true"/>
                            <poNo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Po No (i18N key)"
                                name="./poNo"
                                required="true"/>
                            <amountIncludingTaxes
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Amount Including Taxes &#xa; (i18N key)"
                                name="./amountIncludingTaxes"
                                required="true"/>
                            <reset
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Reset (i18N key)"
                                name="./reset"
                                required="true"/>
                            <results
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Results (i18N key)"
                                name="./results"
                                required="true"/>
                            <pagination
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Pagination (i18N key)"
                                name="./pagination"
                                required="true"/>
                            <noDataFound
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="No Data Found (i18N key)"
                                name="./noDataFound"
                                required="true"/>
                            <paginationFirst
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Pagination First (i18N key)"
                                name="./paginationFirst"
                                required="true"/>
                            <paginationLast
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Pagination Last (i18N key)"
                                name="./paginationLast"
                                required="true"/>
                            <allPaymentsHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="All Payments Heading (i18N key)"
                                name="./allPaymentsHeading"
                                required="true"/>
                            <fetcherror
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Fetch Error (i18N key)"
                                name="./fetcherror"
                                required="true"/>
                            <multipono
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Multiple Po Number (i18N key)"
                                name="./multipono"
                                required="true"/>
                            <paginationprev
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Previous (i18N key)"
                                name="./paginationPrev"
                                required="true"/>
                            <paginationnext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Next (i18N key)"
                                name="./paginationNext"
                                required="true"/>
                            <paid
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Paid (i18N key)"
                                name="./paid"
                                required="true"/>
                        </items>
                    </column1>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
