<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
          jcr:mixinTypes="[rep:AccessControllable]"
          jcr:primaryType="cq:Page">
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <content-default
                            jcr:description="Policy for editable layout containers"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="tetrapak content components"
                            sling:resourceType="wcm/core/components/policy/policy"
                            components="[group:tetrapak-content-components]"
                            policyResourceType="wcm/foundation/components/responsivegrid">
                        <cq:authoring jcr:primaryType="nt:unstructured">
                            <assetToComponentMapping jcr:primaryType="nt:unstructured"/>
                        </cq:authoring>
                        <jcr:content
                                jcr:mixinTypes="[cq:ReplicationStatus]"
                                jcr:primaryType="nt:unstructured"
                                mergeList="{Boolean}true"/>
                    </content-default>
                    <global-default
                            jcr:description="Policy for global components"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="tetrapak global components"
                            sling:resourceType="wcm/core/components/policy/policy"
                            components="group:tetrapak-global-components">
                        <jcr:content jcr:primaryType="nt:unstructured"/>
                    </global-default>
                    <config-default
                            jcr:description="Policy for config components"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="tetrapak configuration components"
                            sling:resourceType="wcm/core/components/policy/policy"
                            components="group:tetrapak-configuration-components">
                        <jcr:content jcr:primaryType="nt:unstructured"/>
                    </config-default>
                </responsivegrid>
            </components>
        </foundation>
    </wcm>
    <rep:policy/>
</jcr:root>
