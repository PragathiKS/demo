<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:template="/conf/supplierportal/settings/wcm/templates/language-page"
            jcr:primaryType="cq:PageContent"
            jcr:title="English"
            sling:resourceType="supplierportal/components/structure/page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                <headerconfiguration
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="supplierportal/components/structure/headerconfiguration"
                        dLogoLink="https://supplier-dev.tetrapak.com/dashboard.html"
                        logoUrl="/content/dam/supplierportal/images/TetraPakLogo.png"
                        mLogoLink="https://supplier-dev.tetrapak.com/dashboard.html"/>
                <footerconfiguration
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="supplierportal/components/structure/footerconfiguration"
                        copyrightText="Copyright">
                    <footerLinks jcr:primaryType="nt:unstructured">
                        <item0
                                jcr:primaryType="nt:unstructured"
                                linkLabel="TetraPak"
                                linkPath="https://www.tetrapak.com/"/>
                        <item1
                                jcr:primaryType="nt:unstructured"
                                linkLabel="Terms &amp; Conditions"
                                linkPath="#"/>
                    </footerLinks>
                </footerconfiguration>
            </responsivegrid>
        </root>
    </jcr:content>
    <dashboard/>
</jcr:root>
