<!DOCTYPE html>
<html lang="en">
<sly data-sly-use.{{fsdId}}Model="ux-model.js" data-sly-use.lib="/apps/plas/components/content/{{fsdId}}/{{fsdId}}-template.html" />
<sly data-sly-test="${ {{fsdId}}Model.requestURI == {{fsdId}}Model.nodePathWithSelector} || {{fsdId}}Model.requestURI == {{fsdId}}Model.pagePathWithSelector}">
    <head>
        <sly data-sly-use.clientLib="/libs/granite/sightly/templates/clientlib.html" />
        <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="template" content="demoTemplate">
        <meta name="pageType" content="GenericPageType">
        <title>{{fsdId}} Component</title>
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;700&display=swap" rel="stylesheet">
        <sly data-sly-call="${clientLib.css @ categories='tp.plas.vendor'}" />
        <sly data-sly-call="${clientLib.css @ categories='tp.plas.default.en'}" />
    </head>
</sly>

<body>
    <input type="hidden" id="isDummyLayout" value="true" />
    <div class="tp-container">
        {{> body}}
    </div>

    <sly data-sly-test="${ {{fsdId}}Model.requestURI == {{fsdId}}Model.nodePathWithSelector} || {{fsdId}}Model.requestURI == {{fsdId}}Model.pagePathWithSelector}"
        data-sly-call="${clientLib.js @ categories='tp.plas.vendor'}" />
    <sly data-sly-test="${ {{fsdId}}Model.requestURI == {{fsdId}}Model.nodePathWithSelector} || {{fsdId}}Model.requestURI == {{fsdId}}Model.pagePathWithSelector}"
        data-sly-call="${clientLib.js @ categories='tp.plas.global'}" />
</body>
</html>
